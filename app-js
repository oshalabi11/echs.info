(function () {
    "use strict";
    
    var moduleObj = moduler('app', {
        init: function (module) {
            $('html').removeClass('js-off').addClass('js-on');

            moduleObj.fallbackSvg();
            moduleObj.iosLandscapeAdjusting();
            moduleObj.formsAssembly();
            $(document).ajaxComplete(function () {
                if ('validator' in $ && 'unobtrusive' in $.validator) {
                    $.validator.unobtrusive.parse('form');
                }
            });

            if ($(window).width() < 599-16) {
                var $social = $('.Social');
                if ($social.length) {
                    var $lastGridCell = $social.parent().next();
                    $social.detach().appendTo($lastGridCell);
                }

               
              
            };
           


        },

        fallbackSvg: function () {
            // IE 8 doesn't support SVG and will load a fallback PNG image instead.
            if (!Modernizr.svg) {
                $("img[src$='.svg']").attr("src", function(i, attr) {
                    return attr.replace('.svg', '.png');
                });

                $('html').addClass('no-svg');
            }
        },
        
        iosLandscapeAdjusting: function () {
            if (navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPad/i) || navigator.userAgent.match(/iPod/i)) {
                var viewportmeta = document.querySelector('meta[name="viewport"]');
                if (viewportmeta) {
                    viewportmeta.content = 'width=device-width, minimum-scale=1.0, maximum-scale=1.0';
                    document.body.addEventListener('gesturestart', function () {
                        viewportmeta.content = 'width=device-width, minimum-scale=0.25, maximum-scale=1.6';
                    }, false);
                }
            }
        },
        // target some of the embeded code for styling
        formsAssembly: function () {
            $('.Form--assembly select').each(function (i, o) {
                if (!$(this).prop('multiple')) {
                    $(this).parent().addClass('Input--select');
                }
            });
            
        }
    });
  
})();

$(window).load(function () {
    if (window.location.hash) {

        $(window).scrollTop($(window).scrollTop() - 115);
    } else {
        // Fragment doesn't exist
    }
});;
(function () {
  "use strict";

  var moduleObj = moduler('back-to-top', {
    defaults: {
      scrollTrigger: 1000, // height after which back to top element is visible
      showClass: 'show' // css class added to back to top button to show it
    },

    init: function (module) {
      $(window).on('scroll', module, moduleObj.listen.backToTop);
      module.$element.on('click', module, moduleObj.listen.animateScrollToTop);

      moduleObj.listen.backToTop(module);
    },

    listen: {
      animateScrollToTop: mo.event(function (module, event) {
        event.preventDefault();
        $('html,body').animate({
          scrollTop: 0
        }, 700, "swing");
      }),
      backToTop: mo.event(function (module) {
        var currentViewHeight = $(window).scrollTop();
        if (currentViewHeight > module.settings.scrollTrigger) {
          module.$element.addClass(module.settings.showClass);
        } else {
          module.$element.removeClass(module.settings.showClass);
        }
      })
    }
  });
})();;
(function () {
    "use strict";

    var moduleObj = moduler('carousel', {
        defaults: {
            columns: {
                mobile: 1,
                tablet: 2,
                desktop: 4
            },
            maxScreenSizes: {
                mobile: 992,
                tablet: 1200
            },
            initialSlide: 0,
            trackSelector: ".Carousel-list",
            itemsSelector: ".Carousel-item",
            controls: true,
            controlsSelector: ".Carousel-controls",
            autoplay: false,
            loop: false,
            announcing: true,
            touch: true
        },

        init: function (module) {
            module.$items = module.$element.find(module.settings.itemsSelector);
            module.$track = module.$element.find(module.settings.trackSelector);

            if(module.settings.controlsSelector) {
                module.$controls = module.$element.find(module.settings.controlsSelector);
                module.$prev = module.$controls.find(".Carousel-prev");
                module.$next = module.$controls.find(".Carousel-next");

                $(module.$prev).click(function() {
                    moduleObj.listen.prev(module);
                });
                $(module.$next).click(function() {
                    moduleObj.listen.next(module);
                });
            }

            // Set all slides as hidden
            moduleObj.toggleSlide(module.$items, false);
            // Check how many items should be displayed and get the size of a single item
            moduleObj.setMode(module);
            moduleObj.setItemSize(module);
            moduleObj.setLiveRegion(module);
            // Active index always points to the first active (as in not-hidden) item. Active items are activeItemIndex + columns
            module.activeItemIndex = module.settings.initialSlide;
            moduleObj.handleSlideChange(module, module.activeItemIndex);

            // module.clickTimeout = null;
            // module.keydownTimeout = null;
            // module.isClick = true;
            module.resizeTimeout = null;
            module.touchmoveTimeout = null;

            $(window).resize(function() {
                module.resizeTimeout = setTimeout(function () {
                    moduleObj.listen.resize(module);
                }, 500);
            });

            // $(document).keydown(function() {
            //     module.keydownTimeout = setTimeout(function () {
            //         module.isClick = false;
            //     }, 100);
            // });
            //
            // $(document).mousedown(function() {
            //     module.clickTimeout = setTimeout(function () {
            //         module.isClick = true;
            //     }, 100);
            // });

            if(module.settings.touch) {
                $(module.$track).on("touchstart", function(e) {
                    moduleObj.handleTouchStart(module, e);
                });

                $(module.$track).on("touchmove", function(e) {
                    module.touchmoveTimeout = setTimeout(function () {
                        moduleObj.handleTouchEnd(module, e);
                    }, 150);
                });
            }

            $(module.$items).find("a").focus(function(e) {
                const wrappingItem = $(this).parents(module.settings.itemsSelector);
                const index = jQuery.inArray(wrappingItem[0], $(module.$items));
                if(index <= module.$items.length - module.settings.columns[module.mode]) moduleObj.handleSlideChange(module, index);
            });
        },
        setLiveRegion: function(module) {
            module.liveRegion = jQuery('<div/>', {
                "class": 'liveregion u-isHiddenVisually',
                "aria-atomic": true,
                "aria-live": "polite"
            });
            module.$element.append(module.liveRegion);
        },
        announceItems: function(module) {
            const s = module.settings.columns[module.mode] > 1 ? "s " : " ";
            let count = module.activeItemIndex + 1;

            if(module.settings.columns[module.mode] > 1) {
                count = module.activeItemIndex + 1 + " to " + (module.activeItemIndex + module.settings.columns[module.mode])
            }

            module.liveRegion.text('Item' + s + count + 'out of ' + module.$items.length);
        },
        handleTouchStart: function (module, e) {
            const touchDown = e.originalEvent.touches[0];
            module.touchDown = touchDown.clientX;
        },
        handleTouchEnd: function (module, e) {
            if (!module.touchDown) {
                return;
            }

            const touchDown = e.originalEvent.touches[0].clientX;
            const delta = module.touchDown - touchDown;

            if (delta > 0) {
                // Swipe right
                moduleObj.listen.next(module);
            } else {
                // Swipe left
                moduleObj.listen.prev(module);
            }

            module.touchDown = null;
        },
        handleSlideChange: function (module, activeSlide) {
            module.activeItemIndex = activeSlide;

            for(let i = module.activeItemIndex; i < module.activeItemIndex + module.settings.columns[module.mode]; i++) {
                moduleObj.toggleSlide(module.$items[i], true);
            }

            $(module.$track).css('transform', 'translateX(' + (-module.activeItemIndex * module.itemSize) + 'px)');

            if(module.settings.controls) moduleObj.updateControls(module);
            if(module.settings.announcing) moduleObj.announceItems(module);
        },
        toggleSlide: function (item, value) {
            $(item).toggleClass("is-active", value);
            $(item).attr("aria-hidden", !value);
        },
        // Sets the mode the carousel is in - mobile / tablet / desktop
        // The component uses that to get the max number of active items
        setMode: function (module) {
            if($(window).innerWidth() > module.settings.maxScreenSizes.mobile) {
                if($(window).innerWidth() > module.settings.maxScreenSizes.tablet) {
                    module.mode = "desktop";
                } else {
                    module.mode = "tablet";
                }
            } else {
                module.mode = "mobile";
            }

            module.slideDelta = module.mode === "desktop" ? 3 : 1;
        },
        // Sets the size of a single carousel item
        setItemSize: function (module) {
            module.itemSize = $(module.$items[0]).outerWidth();
        },
        updateControls: function(module) {
            if(module.settings.loop) {} else {
                if(module.activeItemIndex === 0) {
                    $(module.$prev).attr("disabled", true);
                    $(module.$next).attr("disabled", false);
                    $(module.$next).addClass("Button--orange").removeClass('Button--outline Button--outline-dark');
                    $(module.$prev).addClass('Button--outline Button--outline-dark').removeClass("Button--orange");
                } else if(module.activeItemIndex === module.$items.length - module.settings.columns[module.mode]) {
                    $(module.$prev).attr("disabled", false);
                    $(module.$next).attr("disabled", true);
                    $(module.$prev).addClass("Button--orange").removeClass('Button--outline Button--outline-dark');
                    $(module.$next).addClass('Button--outline Button--outline-dark').removeClass("Button--orange");
                } else {
                    $(module.$prev).attr("disabled", false);
                    $(module.$next).attr("disabled", false);
                    $(module.$next).removeClass("Button--orange").addClass('Button--outline Button--outline-dark');
                    $(module.$prev).removeClass("Button--orange").addClass('Button--outline Button--outline-dark');
                }
            }
        },
        listen: {
            resize: mo.event(function (module) {
                clearTimeout(module.resizeTimeout);
                moduleObj.setMode(module);
                moduleObj.setItemSize(module);
                moduleObj.handleSlideChange(module, module.activeItemIndex);
            }),
            prev: mo.event(function (module) {
                if(module.activeItemIndex > 0) {
                    let item = module.activeItemIndex - module.slideDelta;
                    item < 0  ? item = 0 : null;
                    // Take the number of active items into account
                    moduleObj.toggleSlide(module.$items[item], false);
                    moduleObj.handleSlideChange(module, item);
                }
                if(module.settings.controls) moduleObj.updateControls(module);
            }),
            next: mo.event(function (module) {
                if(module.activeItemIndex < module.$items.length - module.settings.columns[module.mode]) {
                    let item = module.activeItemIndex + module.slideDelta;
                    item > module.$items.length - module.settings.columns[module.mode]  ? item = module.$items.length - module.settings.columns[module.mode] : null;
                    moduleObj.toggleSlide(module.$items[item], false);
                    moduleObj.handleSlideChange(module, item);
                }
                if(module.settings.controls) moduleObj.updateControls(module);
            }),
        },
        destroy: function (module) { }
    })
})();
;
(function () {
    "use strict";

    var moduleObj = moduler('confirm', {
        defaults: {
            message: 'Are you sure you want to perform this action?',
            event: 'click'
        },
        
        init: function (module) {
            module.$element.on(module.settings.event, module, moduleObj.listen.showConfirm);
        },

        listen: {
            showConfirm: mo.event(function (module, e) {
                e.preventDefault();

                if (window.confirm(module.settings.message)) {
                    module.$element.trigger('confirm-yes');
                } else {
                    module.$element.trigger('confirm-no');
                }
            })
        },
        
        destroy: function (module) {
            module.$element.off(module.settings.event, moduleObj.listen.showConfirm);
        }
    });
    
})();;
(function () {
    "use strict";

    var moduleObj = moduler('cookie-checker', {
        defaults: {
            cookieKey: 'acceptCookies'
        },

        init: function (module) {
            if (document.cookie.indexOf(module.settings.cookieKey) < 0) {
                module.$element.slideDown(500);
            }
            module.$element.find(".js-close").on('click', module, moduleObj.listen.hideMessage);
        },

        listen: {
            hideMessage: mo.event(function (module, e) {
                e.preventDefault();

                module.$element.slideUp(500);
                var yearInSeconds = 60*60*24*365;
                document.cookie = escape(module.settings.cookieKey) + "=1; path=/; max-age=" + yearInSeconds;
            })
        },

        destroy: function (module) { }
    });

})();;
(function () {
    "use strict";

    var moduleObj = moduler('country-filter', {
        defaults: {
            data: [],
            countrySelector: '#SearchFields_Country',
            stateSelector: '#SearchFields_State'
        },

        init: function (module) {
            module.$country = $(module.settings.countrySelector);
            module.$state = $(module.settings.stateSelector);

            module.$country.prop('default-values', module.$country.clone().val('').html());

            module.$element.change(function () {
                module.$state.val('').prop('disabled', true);
                moduleObj.refresh(module);
            });

            if (module.$element.val()) {
                var currentCountry = module.$country.val();
                moduleObj.refresh(module);
                module.$country.val(currentCountry);
            }
        },

        refresh: function(module) {
            var region = module.$element.val();

            for (var i in module.settings.data) {
                var item = module.settings.data[i];

                if (item.region == region) {
                    moduleObj.filterCountries(module, item.countries);
                    return;
                }
            }
        },

        filterCountries: function(module, countries) {
            module.$country.html(module.$country.prop('default-values'));

            for (var u in countries) {
                var countryCode = countries[u];
                var countrySelect = module.$country.find('[value=' + countryCode + ']');

                if (countrySelect.length) {
                    countrySelect.addClass('keep');
                }
            }

            module.$country.val('').find('option:not(.keep):not([value=""])').remove();
        }
    });
})();
;
(function () {
    "use strict";

    var moduleObj = moduler('datepicker', {
        defaults: {},
        
        init: function (module) {
        	var datePicker = module.$element.datepicker();
        }
    });
    
})();;
(function () {
    "use strict";

    var moduleObj = moduler('disable-toggler', {
        defaults: {
            regionElement: '',
            countryElement: '',
            startDateElement: '',
            endDateElement: '',
            event: '',
        },
        
        init: function (module) {
            module.$element.find('input[type=checkbox]').on('change', function () {
                moduleObj.updateState(module);
            });
        },

        updateState: function (module) {
            var eventTypes = module.$element;
            var isSelfPaced = eventTypes.find('#SearchFields_TypeSelfPaced');
            var isOnline = eventTypes.find('#SearchFields_TypeOnline');
            var activeElements = eventTypes.find('input[type=checkbox]:checked');
            var isSelfPacedChecked = $(isSelfPaced).is(':checked');
            var isOnlineChecked = $(isOnline).is(':checked');

            $(module.settings.regionElement).prop('disabled', false);
            $(module.settings.countryElement).prop('disabled', false);
            $(module.settings.startDateElement).prop('disabled', false);
            $(module.settings.endDateElement).prop('disabled', false);

            moduleObj.enableEvents($(module.settings.startDateElement));
            moduleObj.enableEvents($(module.settings.endDateElement));


            if (activeElements.length === 0 || activeElements.length > 2) {
                return;
            } 

            if (isOnlineChecked && !isSelfPacedChecked) {
                $(module.settings.regionElement).prop('disabled', true);
                $(module.settings.countryElement).prop('disabled', true);

                return;                   
            } 

            if (!isOnlineChecked && isSelfPacedChecked) {
                $(module.settings.regionElement).prop('disabled', true);
                $(module.settings.countryElement).prop('disabled', true);
                $(module.settings.startDateElement).prop('disabled', true);
                $(module.settings.endDateElement).prop('disabled', true);

                moduleObj.disableEvents($(module.settings.startDateElement));
                moduleObj.disableEvents($(module.settings.endDateElement));

                return;                   
            } 

            if (isOnlineChecked && isSelfPacedChecked) {
                $(module.settings.regionElement).prop('disabled', true);
                $(module.settings.countryElement).prop('disabled', true);

                return
            }
        },

        enableEvents: function (module) {
           $('.cap').remove();

            module.parent()
                .css('background-color', 'hsl(0, 0%, 100%);')
                .css('cursor', 'pointer')
                .css('color', 'hsl(204, 4%, 74%)')
        },
        
        disableEvents: function (module) {
            var eventTarget = module.parent();

             eventTarget.css('position', 'relative')

            $('<div/>', { class: 'cap' })
                .css('width', `${eventTarget.outerWidth()}`)
                .css('height', `${eventTarget.outerHeight()}`)
                .css('position', 'absolute')
                .css('top', '0')
                .css('left', '0')
                .appendTo($(eventTarget))

            module.parent()
                .css('background-color', 'hsl(204, 4%, 74%);')
                .css('cursor', 'not-allowed')
                .css('color', 'hsl(0, 0%, 12%);')
        }
    });
    
})();;
(function () {
    "use strict";

    var moduleObj = moduler('dispatcher', {
        defaults: {
            listenEvent: 'click', // the event to listen for. (required)
            listenSelector: null, // selector for elements to listen for the event (optional)

            triggerEvent: '', // event name to trigger when listenEvent fires. (required)
            triggerSelector: null, // selector for elements to trigger the event. (optional)
            triggerContext: null, // the context in which it will try to find elements through triggerSelector. (optional)

            preventDefault: true // whether to preventDefault when event fires.
        },
        
        init: function (module) {
            var settings = module.settings;

            module.$element.on(settings.listenEvent, settings.listenSelector, module, moduleObj.eventFired);
        },

        eventFired: mo.event(function (module, e) {
            var settings = module.settings;

            if (settings.preventDefault) {
                e.preventDefault();
            }

            if (settings.triggerSelector) {
                var $container = moduleObj.getContext(module);
                $container.find(settings.triggerSelector).trigger(settings.triggerEvent);
            } else {
                module.$element.trigger(settings.triggerEvent);
            }
        }),

        getContext: function (module) {
            var triggerContext = module.settings.triggerContext;
            
            if (triggerContext) {
                if (triggerContext == 'parent') {
                    return module.$element.parent(); 
                }
                return $(triggerContext);
            }
            return module.$element;
        },
        
        destroy: function (module) { 
            module.$element.off(settings.listenEvent, moduleObj.eventFired);
        }
    });
    
})();;
(function () {
    "use strict";

    var moduleObj = moduler('form-cleaner', {
        defaults: {
            event: "submit"
        },
        
        init: function (module) {
            module.$element.on(module.settings.event, module, moduleObj.listen.cleanForm);
        },
        
        listen: {
            cleanForm: mo.event(function(module) {
                module.$element.find("input, textarea").val(function (index, value) {
                    return moduleObj.stripHtml(value);
                });
            })
        },

        stripHtml: function (string) {
            return string.replace(/(<([^>]+)>)/ig, "");
        }
    });
    
})();;
(function($) {
    "use strict";

    var moduleObj = moduler('form-poster', {
        defaults: {
            submitButton: 'input[type=submit], button[type=submit]',
            url: null, // override the form's url. in case you want different url for ajax requests.
            data: null, /* extra data to send along the request to server */
            event: 'submit',
            contentElement: null,
            responseModule: null,
            loadingCssClass: 'form-loading',
            responseSegment: false, // update only a segment of the response (contentElement segment)
            refreshSegments: [],
            scrollIntoView: null, // selector of element to scroll into view after load
            stripHtml: false,
            useHistoryApi: false,
            cookieKey: null // the name of the cookie 
        },

        init: function(module) {
            module.$element.on(module.settings.event, module, moduleObj.listen.sendForm);
        },

        listen: {
            sendForm: mo.event(function (module, e) {
                var $form = module.$element.is('form') ? module.$element : module.$element.find('form'),
                    $submitButton = $form.find(module.settings.submitButton),
                    $contentElement = module.settings.contentElement !== null ? $(module.settings.contentElement) : module.$element,
                    formMethod = $form.attr('method') || 'POST',
                    url = module.settings.url || $form.attr('action');

                e.preventDefault();
                
                // disable button while request is processing
                $submitButton.prop('disabled', true);
                module.$element.addClass(module.settings.loadingCssClass);

                var serializedFormValues = $form.serialize();

                // send request
                $.ajax({
                    type: formMethod,
                    url: url,
                    data: $.extend(moduleObj.serializeFormObject($form, module.settings.stripHtml), module.settings.data)
                }).always(function () {
                    $submitButton.prop('disabled', false);
                    module.$element.removeClass(module.settings.loadingCssClass);
                }).done(function (response, status, xhr) {
                    if (module.settings.useHistoryApi && 'history' in window && 'replaceState' in history && formMethod.toLowerCase() == 'get') {
                        history.replaceState(null, document.title, url + '?' + serializedFormValues);
                    }

                    if (module.settings.cookieKey && 'cookie' in $) {
                        $.cookie(module.settings.cookieKey, decodeURIComponent(url + '?' + serializedFormValues), { path: '/' });
                    }

                    if (module.settings.responseModule) {
                        mo.utils.removeModuleFromElement($contentElement, module.settings.responseModule);
                        mo.utils.addModuleToElement($contentElement, module.settings.responseModule, { response: response });
                    } else if ($contentElement.length) {
                        if (module.settings.responseSegment) {
                            var $response = $($.parseHTML(response, document, true));

                            for (var i in module.settings.refreshSegments) {
                                var segment = module.settings.refreshSegments[i];
                                var $segment = $response.find(segment).add($response.filter(segment));

                                if ($segment.length) {
                                    $(segment).html($segment.html());
                                }
                            }
                        } else {
                            $contentElement.html(response);
                        }
                    }

                    if (module.settings.scrollIntoView) {
                        $(module.settings.scrollIntoView).get(0).scrollIntoView();
                    }

                    module.$element.trigger('form-poster-done', { response: response });
                }).error(function () {
                    module.$element.trigger('form-poster-error');
                });
            })
        },

        serializeFormObject: function($element, stripHtml) {
            var obj = {};
            $.each($element.serializeArray(), function (_, item) {
                var value = (stripHtml) ? moduleObj.stripHtml(item.value) : item.value;
                if (obj.hasOwnProperty(item.name)) {
                    obj[item.name] = $.makeArray(obj[item.name]);
                    obj[item.name].push(value);
                }
                else {
                    obj[item.name] = value;
                }
            });
            return obj;
        },

        destroy: function (module) {
            module.$element.off(module.settings.event, moduleObj.listen.sendForm);
        },

        stripHtml: function(string) {
            return string.replace(/(<([^>]+)>)/ig, "");
        }
    });

})(jQuery);;
(function () {
    "use strict";

    var moduleObj = moduler('form-redirect', {
        defaults: {
            inputSelector: 'input[type=radio]'
        },

        init: function (module) {
            module.$element.find(module.settings.inputSelector).on('change', function(e) {
                location.href = $(this).val();
            });
        }
    });
})();
;
(function () {
    "use strict";

    var moduleObj = moduler('google-analytics', {
        defaults: {
            event: 'click',
            category: 'ga-event',
            action: 'ga-clicked'
        },
        
        init: function (module) {
            module.$element.on(module.settings.event, module, moduleObj.listen.moduleClicked);
        },
        
        listen: {
        	moduleClicked: mo.event(function (module, e) {
                if (!window.hasOwnProperty('ga')) return;

                window['ga']('send', 'event', module.settings.category, module.settings.action);

            })
        },
        
        destroy: function (module) { }
    });
    
})();;
(function () {
    "use strict";

    var moduleObj = moduler('header-hero-slider', {
        defaults: {
            currentSlide: 0,
            slideTime: 10000, // 10s
            transitionDuration: 1300, // durationXL - 1.3s
            hoverTimeout: 3000, // 3s
            showControls: false,
            controlsSelector: ".HeaderHero-controls",
            slides: [{
                image: '/Assets/Images/content/Dwight-405-resize.jpg',
                link: "IB",
                subHeading: 'Education for a better world',
                buttonText: 'Learn why the IB is unique',
                heading: 'The International Baccalaureate (IB) develops lifelong learners who thrive and make a difference.',
                videoEmbed: null,
                element: null
            }]
        },

        init: function (module) {
            module.$subHeading = module.$element.find('.HeaderHero-subHeading');
            module.$heading = module.$element.find('.HeaderHero-heading span');
            module.$link = module.$element.find('.HeaderHero-link');
            module.$image = module.$element.find('.HeaderHero-image--desktop');
            module.$imageMobile = module.$element.find('.HeaderHero-image--mobile');
            module.$button = module.$element.find('.HeaderHero-button span');
            module.scrollTimeout = null;

            moduleObj.showSlide(module, module.settings.slides[module.settings.currentSlide]);

            if(module.settings.slides.length > 1) {
                moduleObj.startSliderLoop(module);
                moduleObj.hoverEvents(module);

                module.$element.hover(function () {
                    moduleObj.stopSliderLoop(module);
                }, function () {
                    if (!module.isPaused) {
                        module.settings.currentSlide = -1;
                        moduleObj.startSliderLoop(module);
                    }
                });

                if(module.settings.showControls) {
                    moduleObj.addControls(module);
                }

                // Change the slide only when the slider is in the viewport
                $(window).bind("scroll.slider", function() {
                    module.scrollTimeout = setTimeout(function () {
                        moduleObj.handleScroll(module);
                    }, 1000);
                });

                moduleObj.handleScroll(module);
            }
        },

        hoverEvents: function(module) {
            for (var i = 0; i < module.settings.slides.length; i++) {
                var slide = module.settings.slides[i];

                if (slide.element) {
                    $(slide.element).hover((function (s) {
                        return function() {
                            moduleObj.stopSliderLoop(module);

                            if (module.hoverTimer) {
                                clearTimeout(module.hoverTimer);
                            }

                            moduleObj.showSlide(module, s);
                        };
                    })(slide), function () {
                        if (module.hoverTimer) {
                            clearTimeout(module.hoverTimer);
                        }

                        // start the loop again after few seconds
                        module.hoverTimer = setTimeout(function () {
                            // reset back to first slide
                            module.settings.currentSlide = 0;

                            moduleObj.showSlide(module, module.settings.slides[module.settings.currentSlide]);
                            moduleObj.startSliderLoop(module);
                        }, module.settings.hoverTimeout);
                    });
                }
            }
        },

        startSliderLoop: function (module, force) {
            const currentSlide = module.settings.currentSlide === -1 ? 0 : module.settings.currentSlide;
            let timing = module.settings.slideTime;

            if (module.timer) {
                clearTimeout(module.timer);
            }

            module.settings.currentSlide += 1;
            const nextSlide = module.settings.currentSlide % module.settings.slides.length;

            if(module.settings.slides[currentSlide] && module.settings.slides[currentSlide].slideTiming) {
                timing = module.settings.slides[currentSlide].slideTiming
            }

            const delay = force ? 0 : timing;

            module.timer = setTimeout(function () {
                moduleObj.showSlide(module, module.settings.slides[nextSlide]);
                moduleObj.setActiveControl(module, nextSlide);
                moduleObj.startSliderLoop(module);
            }, delay);
        },

        stopSliderLoop: function (module) {
            if (module.hoverTimer) {
                clearTimeout(module.hoverTimer);
            }
            if (module.timer) {
                clearTimeout(module.timer);
            }
        },

        showSlide: function (module, slide) {
            if (module.currentSlideObj == slide) {
                return;
            }

            var slideTemplateHtml;

            if (slide.videoEmbed) {
                slideTemplateHtml = $('.js-headerHeroVideoTemplate').html();
            } else {
                slideTemplateHtml = $('.js-headerHeroTemplate').html();
            }

            var $slideElement = $($.parseHTML(slideTemplateHtml));

            var $subHeading = $slideElement.find('.HeaderHero-subHeading');
            var $heading = $slideElement.find('.HeaderHero-heading');
            var $link = $slideElement.find('.HeaderHero-link');
            var $image = $slideElement.find('.HeaderHero-image--desktop');
            var $imageMobile = $slideElement.find('.HeaderHero-image--mobile');
            var $button = $slideElement.find('.HeaderHero-button');

            $subHeading.html(slide.subHeading);
            $heading.find('span').html(slide.heading);
            $link.attr('href', slide.link);
            $image.attr('src', slide.image);
            slide.imageMobile && slide.imageMobile.length ? $imageMobile.attr('src', slide.imageMobile) : $imageMobile.remove();
            slide.buttonText && slide.buttonText.length ? $button.find('span').html(slide.buttonText) : $button.remove();

            $image.addClass('is-transparent');
            $image.on('load', function() {
                $image.removeClass('is-transparent');
            });

            $imageMobile.addClass('is-transparent');
            $imageMobile.on('load', function() {
                $imageMobile.removeClass('is-transparent');
            });

            module.$element.append($slideElement);

            if (slide.videoEmbed) {
                var $embed = $slideElement.find('.js-videoEmbed');
                let embedUrl = slide.videoEmbed;

                embedUrl.includes('?') ? embedUrl += "&" : embedUrl += "?";

                if(embedUrl.includes('vimeo')) {
                    embedUrl += "background=1&controls=0&loop=1&autoplay=1&autopause=0&muted=1&playsinline=1&transparent=1";
                } else if(embedUrl.includes('youtube')) {
                    // Make sure that the mute parameter is last in the param query
                    embedUrl += "controls=0&showinfo=0&disablekb=1&iv_load_policy=3&rel=0&modestbranding=1&loop=1&playsinline=1&autoplay=1&mute=1";
                }

                $embed.attr('src', embedUrl);
            }

            // temporary Fix for image resizing infinitely in the edit mode
            // setTimeout(function() {
            //     if($image.get(0))
            //     imgCoverEffect($image.get(0), {
            //         alignX: 'center',
            //         alignY: 'middle',
            //         watchResize: true
            //     });
            // }, 0);

            $slideElement.fadeIn({
                duration: module.settings.transitionDuration,
                easing: module.settings.easingIn,
                complete: function () {
                    module.currentSlideElement.find('.HeaderHero-video').removeClass('is-transparent');
                }
            });

            if (module.currentSlideElement) {
                module.currentSlideElement
                    .find('.HeaderHero-image').prop('stopResize', true).end()
                    .fadeOut(module.settings.transitionDuration, function () {
                        $(this).remove();
                    });
            }

            module.currentSlideElement = $slideElement;
            module.currentSlideObj = slide;
        },

        handleControlChange: function(module, index) {
            if($(module.controls[index]).is(':checked')) {
                module.settings.currentSlide = index - 1;
                moduleObj.startSliderLoop(module, true);
            }
        },

        setActiveControl: function(module, index) {
            $(module.controls[index]).prop( "checked", true );
        },

        addControls: function(module) {
            module.controls = [];
            const wrapper = $(module.settings.controlsSelector);

            module.settings.slides.forEach((slide, index) => {
                let input = $('<input type="radio" name="active-slide">')
                        .attr({ value : index })
                        .attr({ id : "slide-" + index })
                        .attr({ checked: module.settings.currentSlide - 1 === index });
                input.on("change", function(){
                    moduleObj.handleControlChange(module, index);
                });

                module.controls.push(input);
                wrapper.append(input);

                wrapper.append($("<label for='slide-" + index + "' ></label>"));
            });
        },

        checkIfInViewport: function(module) {
            var elementTop = $(module.$element).offset().top;
            var elementBottom = elementTop + $(module.$element).outerHeight();

            var viewportTop = $(window).scrollTop();
            var viewportBottom = viewportTop + $(window).height();

            return elementBottom > viewportTop && elementTop < viewportBottom;
        },

        handleScroll: function (module) {
            if(moduleObj.checkIfInViewport(module)) {
                if (!module.isPaused) {
                    module.settings.currentSlide = -1;
                    moduleObj.startSliderLoop(module);
                }
            } else {
                moduleObj.stopSliderLoop(module);
            }
        }
    });
})();
;
(function () {
    "use strict";
    
    /*
        Example HTML:

        <div class="products">
            <!-- list products for page one. -->
        </div>

        <p>
            <a href="?page=2" data-module="load-more" data-load-more="url: '/api/products', contentElement: '.products'">Load results {from}-{to}</a>
        </p>
    */

    var moduleObj = moduler('load-more', {
        defaults: {
            url: null,
            event: 'click',
            data: null, /* extra data to send along the request to server */
            contentElement: null, /* selector for element where content should be appended or replaced */
            page: 0, /* the page currently on */
            insertMode : 'append', /* append|replace */
            loadingCssClass: 'is-loading',
            pageSize: null, // size of each page, used when displaying page range in button. ("Results from 11-20")
            totalItemCount: null, // total number of items in the list.
            responseSegment: false, // update only a segment of the response (contentElement segment)
            refreshSegments: [],
            useHistoryApi: false,
            cookieKey: null
        },

        init: function (module) {
            if (!module.settings.url) {
                if (module.$element.is('a[href]')) {
                    module.settings.url = module.$element.attr('href');
                } else {
                    module.settings.url = window.location.href;
                }
            }
            
            // save a reference to contentElement
            module.$contentElement = module.settings.contentElement !== null ? $(module.settings.contentElement) : module.$element;

            module.$element.on(module.settings.event, module, moduleObj.listen.loadMore);

            var linkHtml = module.$element.html();
            var shouldDisplayPageRange = linkHtml.indexOf('{from}') != -1;

            if (shouldDisplayPageRange) {
                module.shouldDisplayPageRange = true;
                module.linkHtml = linkHtml;

                moduleObj.updateLinkText(module);
            }
        },

        loadPage: function (module) {
            module.$element.addClass(module.settings.loadingCssClass);
            module.isLoading = true;

            $.ajax({
                type: 'GET',
                url: module.settings.url.replace('{page}', module.settings.page),
                data: $.extend({
                    partial: true,
                    page: module.settings.page
                }, module.settings.data)
            })
            .always(function () {
                module.$element.removeClass(module.settings.loadingCssClass);
                module.isLoading = false;
            })
            .done(function (response, status, xhr) {
                var pagingUrl = module.settings.url.replace('{page}', module.settings.page) + (module.settings.url.indexOf('?') ? '&' : '?') + $.param({ page: module.settings.page });

                if (module.settings.useHistoryApi && 'history' in window && 'replaceState' in history) {
                    history.replaceState(null, document.title, pagingUrl);
                }

                if (module.settings.cookieKey && 'cookie' in $) {
                    $.cookie(module.settings.cookieKey, decodeURIComponent(pagingUrl), { path: '/' });
                }

                
                    if (module.settings.responseSegment) {
                        var $response = $($.parseHTML(response, document, true));

                        for (var i in module.settings.refreshSegments) {
                            var segment = module.settings.refreshSegments[i];
                            var $segment = $response.find(segment).add($response.filter(segment));

                            if ($segment.length) {
                            	if (module.settings.insertMode == 'replace') {
                            		$(segment).html($segment.html());
                            	} else if (module.settings.insertMode == 'append') {
                            		$(segment).append($segment.html());
                            	}
                            }
                        }
                    } else {
                    	if (module.settings.insertMode == 'replace') {
                    		module.$contentElement.html(response);
                    	} else if (module.settings.insertMode == 'append') {
                    		module.$contentElement.append(response);
                    	}
                        
                    }
              

                if (xhr.getResponseHeader('X-LastPage')) {
                    // hide the load more button instead of removing it so that 
                    // other modules can still trigger events
                    module.$element.hide();
                }

                if (module.shouldDisplayPageRange) {
                    moduleObj.updateLinkText(module);
                }

                module.$element.trigger('load-more-done', { response: response });
            })
            .error(function () {
                module.$element.trigger('load-more-error');
            });  
        },

        updateLinkText: function(module) {
            var from = (module.settings.page * module.settings.pageSize);
            var nextTo = from + module.settings.pageSize;

            if (module.settings.totalItemCount && nextTo > module.settings.totalItemCount) {
                nextTo = module.settings.totalItemCount;
            }
            
            var newLinkText = module.linkHtml
                .replace('{from}', from + 1)
                .replace('{to}', nextTo);

            module.$element.html(newLinkText);
        },

        listen: {
            loadFirstPage: mo.event(function (module) {
                module.settings.page = 1;

                module.$element.show();
                module.$contentElement.empty();

                moduleObj.loadPage(module);
            }),

            loadMore: mo.event(function (module, e) {
                e.preventDefault();

                // prevent additional requests when user spam-clicks
                if (module.isLoading) {
                    return;
                }
                
                // increase page by one
                module.settings.page += 1;
                moduleObj.loadPage(module);
            })
        },

        destroy: function (module) {
            module.$element.off(module.settings.event, moduleObj.listen.loadMore);
        }
    });
    
})();;
(function () {
    "use strict";

    var moduleObj = moduler('loader', {
        defaults: {
            url: '',
            contentElement: null,
            event: 'click',
            once: true,
            responseModule: null,
            loadOnInit: false,
            loadingCssClass: 'loading',
            insertMode: 'replace' /* replace|append */
        },
        
        init: function (module) {
            var settings = module.settings;
            
            module.$contentElement = module.settings.contentElement !== null ? $(module.settings.contentElement) : module.$element;

            if (settings.event) {
                module.$element.on(settings.event + '.loader', module, moduleObj.listen.sendRequest);
            }

            if (settings.loadOnInit) {
                moduleObj.listen.sendRequest(module);
            }
        },

        updateElementContent: function (html, module) {
            if (module.settings.insertMode == 'replace') {
                module.$contentElement.html(html);
            } else if (module.settings.insertMode == 'append') {
                module.$contentElement.append(html);
            }
        },
        
        listen: {
            sendRequest: mo.event(function(module) {
                var settings = module.settings,
                    element = module.$element,
                    $contentElement = module.$contentElement;

                if (settings.once && module.isLoaded)
                    return;

                element.addClass(settings.loadingCssClass);
                element.add(module.$contentElement).trigger('loader-loading');

                $.ajax({
                    url: settings.url
                }).always(function () {
                    element.removeClass(settings.loadingCssClass);
                    
                    // trigger the module prefixed event so that other modules can use it.
                    element.trigger('loader-' + settings.event);
                }).done(function (response, status, xhr) {
                    if (module.settings.responseModule) {
                        mo.utils.removeModuleFromElement($contentElement, module.settings.responseModule);
                        mo.utils.addModuleToElement($contentElement, module.settings.responseModule, { response: response });
                    } else if (xhr.getResponseHeader('content-type').indexOf('text/html') !== -1) {
                        moduleObj.updateElementContent(response, module);
                    }

                    module.isLoaded = true;
                    element.add(module.$contentElement).trigger('loader-done', { response: response });
                }).error(function(response) {
                    if (!module.settings.responseModule) {
                        moduleObj.updateElementContent(response, module);
                    }

                    element.add(module.$contentElement).trigger('loader-error', { response: response });
                });

                if (settings.once && settings.event) {
                    $(this).off(settings.event + '.loader').on(settings.event + '.loader', function () {
                        element.trigger('loader.' + settings.event);
                    });
                }
            })
        }
    });
})();;
(function () {
	"use strict";

	var moduleObj = moduler('login', {
		defaults: {
			loginUrl: 'https://ib.app2.local/',
			logoutUrl: 'https://ib.app2.local/',
			loginTxt: 'Login',
			logoutTxt: 'Logout',
			jsEndpoint: 'https://ib.app2.local/js/user' //'https://sso.ibo.org/login.js'
		},

		init: function (module) {
		    module.$element.attr('href', module.settings.loginUrl);

			$.ajax({
				dataType: 'jsonp',
				url: module.settings.jsEndpoint,
				success: function (data) {
					moduleObj.listen.successCallback(data, module);
				},
				error: function () {
					module.$element.text(module.settings.loginTxt);
					moduleObj.deleteLoginCookie();
				}
			});
		},
		setLoginCookie: function () {
			document.cookie = "loggedIn=true";
		},
		deleteLoginCookie: function () {
			var now = new Date();
			now.setHours(now.getHours() - 24);
			document.cookie = "loggedIn=; expires=" + now.toUTCString();

		},
		listen: {
			successCallback: function (data, module) {
				if (typeof data != "undefined") {
					module.$element.text(module.settings.logoutTxt);
					module.$element.attr('href', module.settings.logoutUrl);
					moduleObj.setLoginCookie();
				}
				else {
					module.$element.text(module.settings.loginTxt);
					moduleObj.deleteLoginCookie();
				}
			}
		}
	});

})();
;
(function () {
    "use strict";

    // NOTE This is the new Map module that should be used for all new development. When possible the old map module (map.js) should be removed.

    var moduleObj = moduler('map-2', {
        defaults: {
            apiKey: '', // optional
            apiScriptUrl: 'https://maps.googleapis.com/maps/api/js?key=AIzaSyDoIXzVugpsgY-mUloRgCoP-y4gV0FP6RA&callback={callback}',
            markerClustererUrl: '/assets/scripts/vendor/js-markerclustererplus.min.js', // will only be loaded when useMarkerClusterer is true and the library hasn't been loaded yet.
            data: null, // object or array of location:{ address, lat, lng }
            dataUrl: null, // url to json endpoint returning array of location:{ address, lat, lng }
            showMarkers: true,
            showInfoWindow: false,
            useMarkerClusterer: false,
            filter: null, // a string (name of the property we're filtering)
            filterSelector: ".Map-filters-radio",
            center: null, // point of center for map, { address, lat, lng }, (optional)
            zoom: 20,
            maxZoom: null, // can be used when displaying multiple markers (optional)
            infoWindowTemplate: '<aside class="Map-info"><a href="{url}" class="Link--silent Link--arrow-left"><h3 class="Text Text--tag u-marginBottomS"><span>{title}</span></h3><p class="u-marginBottomS">{description}</p><svg class="Icon Icon--smaller u-marginZero" aria-hidden="true"><use xlink:href="#arrow-right"/></svg></a></aside>',
            preventShowOnIE8: false,
            preventShowOnMobile: false,
            mobileBreakpoint: 992,
            styles: {
                pinImage: '/Assets/Images/pin.svg',
                // paste in map style code, e.g. http://snazzymaps.com/style/150/old-school-maps-posters
                mapStyle: [{ "featureType": "landscape", "stylers": [{ "color": "#e9e8e7" }] }, { "featureType": "poi", "stylers": [{ "visibility": "off" }] }, { "featureType": "road", "stylers": [{ "saturation": -100 }, { "lightness": 45 }] }, { "featureType": "administrative", "stylers": [{ "lightness": 34 }] }, { "featureType": "transit", "stylers": [{ "saturation": -100 }, { "lightness": 9 }, { "visibility": "off" }] }, { "featureType": "water", "stylers": [{ "hue": "#007fff" }, { "saturation": -32 }] }, {}], // paste in map style code, e.g. http://snazzymaps.com/style/150/old-school-maps-posters

                // All styles for the map, including the clusters, are in the Map.less file
                // docs: https://googlemaps.github.io/js-markerclustererplus/index.html
                clusterStyles: [{
                    height: 46,
                    width: 46
                }]
            }
        },

        DataType: {
            multiple: 'multiple',
            single: 'single',
            service: 'service'
        },

        init: function (module) {
            module.isInitialized = false;

            if (module.settings.preventShowOnIE8 && layoutEngine.vendor == 'ie' && layoutEngine.version == 8) {
                return;
            }

            if(module.settings.preventShowOnMobile && $(window).innerWidth() <= module.settings.mobileBreakpoint) {
                return;
            }

            module.scrollTimeout = null;

            if(module.settings.filter) {
                // Get the filter controls
                const filterElements = $(module.$element).parent().find(module.settings.filterSelector);

                if(filterElements) {
                    // Set the first one as active
                    module.activeFilter = filterElements[0].value;

                    filterElements.each(function(index) {
                        // Listen for filter changes
                        $(filterElements[index]).change(function() {
                            moduleObj.filterPositions(module, this.value);
                        });
                    });
                }
            }

            // Init the map only when it's in the viewport
            $(window).bind("scroll.map", function() {
                module.scrollTimeout = setTimeout(function () {
                    moduleObj.handleScroll(module);
                }, 300);
            });

            moduleObj.handleScroll(module);
        },

        mapsReady: function (module) {
            $(module.$element).removeClass("is-loading");

            module.dataType = moduleObj.getDataType(module);

            if (module.dataType === moduleObj.DataType.single) {
                var positions = [module.settings.data];
                moduleObj.displayPositions(module, positions);
            }
            else if (module.dataType === moduleObj.DataType.multiple) {
                moduleObj.displayPositions(module, module.settings.data);
            }
            else if (module.dataType === moduleObj.DataType.service) {
                $.getJSON(module.settings.dataUrl).done(function (response) {
                    moduleObj.displayPositions(module, response);
                });
            }
        },

        filterPositions: function (module, value) {
            if(module.markers) {
                $(module.$element).addClass("is-loading");
                var filteredMarkers = [];

                // Set visibility for markers
                module.markers.forEach(item => {
                    let isActive = value == item[module.settings.filter];
                    item.setVisible(isActive);
                    if(isActive) {
                        filteredMarkers.push(item);
                    }
                });

                // Update marker clusterer
                if (module.settings.useMarkerClusterer) {
                    module.markerCluster.clearMarkers();
                    module.markerCluster.addMarkers(filteredMarkers);
                    module.markerCluster.repaint();
                }

                $(module.$element).removeClass("is-loading");
            }
        },

        displayPositions: function (module, data) {
            var deferres = [];

            for (var i in data) {
                var item = data[i];

                deferres.push(moduleObj.getPosition(item));
            }

            $.when.apply($, deferres).done(function () {
                var positions = $.makeArray(arguments);
                var isSinglePosition = positions.length == 1;

                // https://developers.google.com/maps/documentation/javascript/reference/map#MapOptions
                var mapOptions = {
                    zoom: module.settings.zoom,
                    maxZoom: module.settings.maxZoom,
                    styles: module.settings.styles.mapStyle,

                    mapTypeId: google.maps.MapTypeId.ROADMAP,
                    panControl: false,
                    zoomControl: false,
                    fullscreenControl: false,
                    scaleControl: false,
                    streetViewControl: false,
                    scrollWheel: false,
                    mapTypeControl: false
                };

                if (isSinglePosition && !module.settings.center) {
                    mapOptions.center = positions[0];
                }

                var map = new google.maps.Map(module.element, mapOptions),
                    bounds = new google.maps.LatLngBounds(),
                    infoWindow = new google.maps.InfoWindow();
                module.markers = []; // array is only needed when using markerClusterer

                for (var i in positions) {
                    var position = positions[i];
                    bounds.extend(position);

                    if (module.settings.showMarkers) {
                        var marker = moduleObj.addMarkerAndOverlayToMap(module, position, infoWindow, map);
                        module.markers.push(marker);
                    }
                }

                if (module.settings.useMarkerClusterer) {
                    var clusterOptions = {
                        styles: module.settings.styles.clusterStyles,
                        clusterClass: 'Map-cluster',
                    };
                    module.markerCluster = new MarkerClusterer(map, module.markers, clusterOptions);
                }

                if (module.settings.center) {
                    moduleObj.getPosition(module.settings.center).done(function (position) {
                        map.setCenter(position);
                    });
                } else if (!isSinglePosition) {
                    map.fitBounds(bounds);
                }

                moduleObj.filterPositions(module, module.activeFilter);
            });
        },

        addMarkerAndOverlayToMap: function (module, position, infoWindow, map) {
            var marker = new google.maps.Marker({
                position: position,
                title: moduleObj.decode(position.title),
                map: map,
                clickable: module.settings.showInfoWindow,
                icon: module.settings.styles.pinImage,
                type: position.mapItemType,
                visible: module.activeFilter ? position.type == module.activeFilter : true
            });

            if (module.settings.showInfoWindow) {
                google.maps.event.addListener(marker, 'click', function () {

                    var content = moduleObj.transformTemplate(module.settings.infoWindowTemplate, position);

                    if (position.additionalEvents != null) {
                        var extraResults = "";
                        for (var i = 0; i < position.additionalEvents.length; i++) {
                            extraResults += "<br />" + moduleObj.transformTemplate(module.settings.infoWindowTemplate, position.additionalEvents[i]);
                        }
                        content = content + extraResults;
                    }

                    infoWindow.setContent(content);
                    infoWindow.open(map, marker);
                });

            }

            return marker;
        },

        getPosition: function (location) {
            var dfd = $.Deferred();

            if (location.lat && location.lng) {
                var position = new google.maps.LatLng(location.lat, location.lng);
                dfd.resolve(moduleObj.extendPositionWithData(position, location));
            }
            else if (moduleObj.getCachedPosition(location.address)) {
                var position = moduleObj.getCachedPosition(location.address);
                dfd.resolve(moduleObj.extendPositionWithData(position, location));
            }
            else if (location.address) {
                var geocoder = new google.maps.Geocoder();
                var requestInfo = {
                    address: decodeURIComponent(location.address)
                };

                geocoder.geocode(requestInfo, function (results, status) {
                    if (status == google.maps.GeocoderStatus.OK) {
                        var position = results[0].geometry.location;
                        moduleObj.addPositionToCache(location.address, position);
                        if (position) {
                            dfd.resolve(moduleObj.extendPositionWithData(position, location));
                            return dfd;
                        }
                    }

                    console.warn('geocoder lookup failed', status, location.address);
                    dfd.reject({ error: status, address: location.address });
                });
            }

            return dfd.promise();
        },

        extendPositionWithData: function (position, locationInfo) {
            locationInfo.lat = undefined;
            locationInfo.lng = undefined;

            return $.extend(position, locationInfo);
        },

        getDataType: function (module) {
            var dataType = module.settings.data && $.isArray(module.settings.data) ?
                                moduleObj.DataType.multiple : moduleObj.DataType.single;

            if (module.settings.dataUrl) {
                dataType = moduleObj.DataType.service;
            }

            return dataType;
        },

        setData: function(module) {
            var apiScriptPromise = mo.utils.loadScript(module.settings.apiScriptUrl, ('google' in window && 'maps' in google)),
                markerClustererPromise = module.settings.useMarkerClusterer ? mo.utils.loadScript(module.settings.markerClustererUrl, ('MarkerClusterer' in window)) : {};

            // markerClusterer will only be loaded when useMarkerClusterer is set to true.
            $.when(apiScriptPromise, markerClustererPromise).done(function () {
                moduleObj.mapsReady(module);
            });
        },

        transformTemplate: function (template, data) {
            var result = template;
            for (var key in data) {
                result = result.replace('{' + key + '}', moduleObj.decode(data[key]));
            }
            return result;
        },

        decode: function (source) {
            if (source) {
                return decodeURIComponent(source).replace(/\+/g, ' ');
            }
            return '';
        },

        getCachedPosition: function (address) {
            var source = moduleObj;
            source.positions = source.positions || [];
            return source.positions[address];
            //var posString = sessionStorage.getItem(address);
            //return JSON.parse(posString);
        },

        addPositionToCache: function (address, position) {
            var source = moduleObj;
            source.positions = source.positions || [];
            source.positions[address] = position;
            //var posString = JSON.stringify(position);
            //sessionStorage.setItem(address, posString);
        },

        checkIfInViewport: function(module) {
            var elementTop = $(module.$element).offset().top;
            var elementBottom = elementTop + $(module.$element).outerHeight();

            var viewportTop = $(window).scrollTop();
            var viewportBottom = viewportTop + $(window).height();

            return elementBottom > viewportTop && elementTop < viewportBottom;
        },

        handleScroll: function (module) {
            if(!module.isInitialized && moduleObj.checkIfInViewport(module)) {
                moduleObj.setData(module);
                clearTimeout(module.scrollTimeout);
                $(window).unbind('scroll.map');
                module.isInitialized = true;
            }
        }
    });

})();
;
(function () {
    "use strict";

    var moduleObj = moduler('map', {
        defaults: {
            apiKey: 'AIzaSyDoIXzVugpsgY-mUloRgCoP-y4gV0FP6RA',
            lat: 51.347973,
            lng: -0.401345,
            center: null, // { lat: 51, lng: 0 }
            geocoder: null, // lookup and fit bounds to an address
            url: 'https://maps.googleapis.com/maps/api/js?key={APIKEY}&callback={CALLBACK}',
            zoom: 13,
            showMarker: true,
            preventShowOnIE8: false
        },

        init: function (module) {
            if (module.settings.preventShowOnIE8 && layoutEngine.vendor == 'ie' && layoutEngine.version == 8) {
              return;
            }

            moduleObj.loadScript(module, function () {
                var location = new google.maps.LatLng(module.settings.lat, module.settings.lng);
                var center = location;

                if (module.settings.center != null) {
                    center = new google.maps.LatLng(module.settings.center.lat, module.settings.center.lng);
                }

                var map = new google.maps.Map(module.element, {
                    mapTypeId: google.maps.MapTypeId.ROADMAP,
                    panControl: false,
                    zoomControl: true,
                    zoomControlOptions: {
                        style: google.maps.ZoomControlStyle.SMALL,
                        position: google.maps.ControlPosition.LEFT_CENTER
                    },
                    scaleControl: false,
                    streetViewControl: false,
                    scrollwheel: false,
                    mapTypeControl: false,
                    center: center,
                    zoom: module.settings.zoom,

                    styles: [{ "featureType": "landscape", "stylers": [{ "color": "#e9e8e7" }] }, { "featureType": "poi", "stylers": [{ "visibility": "off" }] }, { "featureType": "road", "stylers": [{ "saturation": -100 }, { "lightness": 45 }] }, { "featureType": "administrative", "stylers": [{ "lightness": 34 }] }, { "featureType": "transit", "stylers": [{ "saturation": -100 }, { "lightness": 9 }, { "visibility": "off" }] }, { "featureType": "water", "stylers": [{ "hue": "#007fff" }, { "saturation": -32 }] }, {}]
                });
                var marker = null;

                if (module.settings.geocoder) {
                    var geocoder = new google.maps.Geocoder();
                    var request = { address: decodeURIComponent(module.settings.geocoder) };

                    if (module.settings.countryCode) {
                        request.componentRestrictions = { country: module.settings.countryCode }
                    }

                    geocoder.geocode(request, function (results, status) {
                        //console.log(results, status, results[0].geometry.location);
                        if (status == google.maps.GeocoderStatus.OK) {
                            location = results[0].geometry.location;

                            if (location) {
                                map.setCenter(location);

                                if (module.settings.showMarker) {
                                    marker.setPosition(location);
                                }
                            }

                            if (results[0].geometry.bounds) {
                                map.fitBounds(results[0].geometry.bounds);
                            }
                        } else {
                            map.setZoom(2);
                        }
                    });
                }

                if (module.settings.showMarker) {
                    marker = new google.maps.Marker({
                        position: location,
                        title: module.settings.title,
                        map: map,
                        clickable: false,
                        icon: '/Assets/Images/pin.svg'
                    });
                }
            });

        },

        loadScript: function (module, callback) {
            if ($(document).prop("loading-google-maps")) {
                $(document).bind("google-maps-loaded", function () {
                    callback();
                });
                return;
            }

            var callbackName = 'callback' + Math.random().toString().replace('.', '');
            var loadScriptInternal = function (url) {
                var script = document.createElement('script');
                script.src = url;
                document.body.appendChild(script);
                $(module.$element).removeClass("is-loading");
            }

            window[callbackName] = function () {
                callback();
                $(document).trigger('google-maps-loaded');
            }

            $(document).prop("loading-google-maps", true);
            loadScriptInternal(module.settings.url.replace('{APIKEY}', module.settings.apiKey).replace('{CALLBACK}', callbackName));
        }
    });

})();
;
(function () {
    "use strict";

    var moduleObj = moduler('max-height', {
        defaults: {
            heightElement: ''
        },

        init: function (module) {
            var resizeFired = false;
            $(window).resize(function(event) {
                if (!resizeFired) {
                    resizeFired = true;
                    setTimeout(resizePage, 50);
                }
            });

            function resizePage() {
                // if 'heightElement' is set up, its height will be used
                var heightElement;

                if(module.settings.heightElement) {
                    heightElement = $("." + module.settings.heightElement);
                }

                if (!heightElement || !heightElement.length) {
                    heightElement = module.$element;
                }

                module.$element.css("max-height", getHeight(heightElement));

                resizeFired = false;
            }

            resizePage();

            function getHeight(element) {
                return element[0].scrollHeight + "px";
            }
        }
    });

})();
;
(function ($) {
    "use strict";

    var moduleObj = moduler('mega-menu', {
        defaults: {
            activeElement: null,
            activeElementMenu: null,
            contentElement: null,
            cssClass: 'is-expanded',
            event: '',
            insertMode: 'replace' /* replace|append */,
            itemClass: 'MainNav-item',
            linkClass: 'MainNav-pageLink',
            menuClass: 'MainNav-list',
            screenMaxWidth: 1200
        },

        init: function (module) {
            var megaMenuItems;
            var settings = module.settings;

                module.$contentElement = module.settings.contentElement !== null ? $(module.settings.contentElement) : module.$element;

                if (settings.response) {
                    moduleObj.updateElementContent(settings.response, module);
                }

            if($(window).width() > module.settings.screenMaxWidth) {

                if (module.settings.itemClass) {
                    megaMenuItems = $("." + module.settings.itemClass);

                    megaMenuItems.on('mouseover', function (e) {
                        module.$activeElement = $(this);
                        module.$activeElementMenu = $(this).children(module.settings.menuClass);
                        module.$activeElementMenu.attr("aria-hidden", false);
                        moduleObj.listen.show(module);
                    });

                    megaMenuItems.on('mouseout', function (e) {
                        moduleObj.listen.hide(module);
                    });

                    megaMenuItems.each(function (item) {
                        if (module.settings.linkClass) {
                            var link;
                            link = $("." + module.settings.linkClass);
                        }

                        if (module.settings.menuClass) {
                            var menu;
                            menu = $("." + module.settings.menuClass);
                        }
                    });

                    megaMenuItems.on('blur', function (e) {});
                }

                $(window).on('scroll', function (e) {
                    moduleObj.listen.hide(module);
                });
            }
        },

        updateElementContent: function (html, module) {
            if (module.settings.insertMode == 'replace') {
                module.$contentElement.html(html);
            } else if (module.settings.insertMode == 'append') {
                module.$contentElement.append(html);
            }
        },

        listen: {
            show: mo.event(function (module) {
                module.$activeElement.addClass(module.settings.cssClass);
            }),
            hide: mo.event(function (module) {
                module.$activeElement ? module.$activeElement.removeClass(module.settings.cssClass) : null;
                module.$activeElementMenu ? module.$activeElementMenu.attr("aria-hidden", true) : null;

                module.$activeElement = null;
                module.$activeElementMenu = null;
            }),
        }
    });

})(jQuery);
;
(function () {
    "use strict";

    var moduleObj = moduler('menu-closer', {
        init: function (module) {
            var menu = mo.utils.getModule(module.element, 'menu');

            moduleObj.listenOnScroll(module, function () {
                if (menu.$contentElement.hasClass(menu.settings.cssClass)) {
                    if (menu.$contentElement.css('position') == 'static') {
                        menu.$contentElement.removeClass(menu.settings.cssClass);
                    }
                }
            });

            menu.$contentElement.one('menu-toggled', function () {
                moduleObj.startScrollEvent(module);
            });
        },
        
        startScrollEvent: function (module) {
            $(window).on('scroll.menu-closer', function () {
                module.hasScrolled = true;
            }).trigger('scroll.menu-closer');
        },

        listenOnScroll: function (module, callback) {
            if (module.scrollTimer) {
                clearInterval(module.scrollTimer);
            }

            module.scrollTimer = setInterval(function () {
                if (module.hasScrolled) {
                    module.hasScrolled = false;
                    callback();
                }
            }, 400);
        }
    });
    
})();;
(function ($) {
    "use strict";

    var moduleObj = moduler('menu', {
        defaults: {
            event: 'click',
            contentElement: null,
            menuClass: null,
            cssClass: 'is-active',
            extraElement: null,
            extraClass: null,
            preventDefault: true,
            once: false,
            classToRemove: '',
            elementToRemoveFrom: null,
            eventWrapperSelector: 'ui-menu-item-wrapper'
        },

        init: function (module) {
            module.elementToRemoveFrom = $(module.settings.elementToRemoveFrom);
            module.$element.on(module.settings.event, module, moduleObj.listen.toggleVisibility);
            module.$contentElement = $(module.settings.contentElement ? module.settings.contentElement : module.element);
            module.$eventWrapper = $(module.settings.eventWrapperSelector);

            document.querySelector(module.settings.contentElement ? module.settings.contentElement : module.element).addEventListener('click', function (event) {
                // menu button for mobile has contentElement = html
                // This is a BUG. It affects GTM and other click event listeners
                // This is a workaround, but it needs to be refactored
                if (event.currentTarget.nodeName !== 'HTML') {
                    event.stopPropagation();
                }
            })

            document.addEventListener('click', function (e) {
                if($(e.target).parents('.Navigation').length) return;
                if (e && e.target && !(e.target.classList.contains('ui-menu-item') || e.target.classList.contains('ui-menu-item-wrapper'))) {
                    moduleObj.listen.hide(module);
                }
            })

            module.$contentElement.on('menu-toggled', function (e) {
                //add timeout to wat fr the input to render
                setTimeout(function () {
                    module.$contentElement.find('input[name="search"]').focus();
                }, 500);
            });
        },

        listen: {
            show: mo.event(function (module) {
                module.$contentElement.addClass(module.settings.cssClass);
                module.$contentElement.trigger('menu-shown');

            }),

            hide: mo.event(function (module) {
                module.$contentElement.removeClass(module.settings.cssClass);
                module.$contentElement.trigger('menu-hidden');
            }),

            toggleVisibility: mo.event(function (module, e) {
                if(module.settings.cssClass !== module.settings.classToRemove && module.elementToRemoveFrom.length && module.settings.classToRemove.length) {
                    module.elementToRemoveFrom.removeClass(module.settings.classToRemove);
                }

                if (module.settings.preventDefault) {
                    e.preventDefault();
                }

                e.stopPropagation();

                if (module.settings.once && module.toggled) {
                    return;
                } else if (module.settings.once) {
                    module.toggled = true;
                }

                module.$element.toggleClass(module.settings.menuClass);
                module.$contentElement.toggleClass(module.settings.cssClass);
                $(module.settings.extraElement).toggleClass(module.settings.extraClass);
                module.$contentElement.trigger('menu-toggled');
            })
        }
    });

})(jQuery);
;
(function ($) {
    "use strict";

    var moduleObj = moduler('mobile-menu', {
        defaults: {
            event: 'loader-done',
            currentPage: null,
            screenMaxWidth: 1200
        },

        init: function (module) {
            module.fontSize = 18; // The Andreas' Constant
            module.depth = 0;

            if($(window).width() <= module.settings.screenMaxWidth) {
                module.$element.on('loader-loading', function() {
                    var rootNavList = module.$element.find('.MainNav-list').eq(0);

                    rootNavList.addClass('is-active');
                    module.$element.css('min-height', (rootNavList.innerHeight() / module.fontSize) + 'em');
                });

                module.$element.on(module.settings.event, function () {
                    var rootNavList = module.$element.find('.MainNav-list').eq(0);

                    rootNavList.addClass('is-active');
                    rootNavList.find('.MainNav-list').prepend('<li class="MainNav-item"><button type="button" class="Button Button--plain MainNav-pageLink MainNav-goBack"><svg class="Icon Icon--smaller" aria-hidden="true"><use xlink:href="#chevron-left" /></svg> Back</button></li>');

                    module.$element.css('min-height', (rootNavList.innerHeight() / module.fontSize) + 'em');

                    // set active menu
                    moduleObj.showCurrentPageNode(module);

                    module.$element.on('click', '.MainNav-drillButton', function (e) {
                        e.preventDefault();

                        module.depth += 1;
                        var nextMenu = $(this).next();
                        nextMenu.addClass('is-active');

                        module.$element.css('min-height', (nextMenu.innerHeight() / module.fontSize) + 'em');

                        module.$element.toggleClass('is-nested', module.depth>0);
                        $('html').toggleClass('is-nested-nav', module.depth > 0);
                    });

                    module.$element.on('click', '.MainNav-goBack', function (e) {
                        e.preventDefault();

                        module.depth -= 1;

                        module.$element.css('min-height', (rootNavList.innerHeight() / module.fontSize) + 'em');

                        var currentMenu = $(this).closest('.MainNav-list'),
                            prevMenu = $(this).parents('.MainNav-list').eq(1);

                        currentMenu.removeClass('is-active');

                        if (module.depth > 0) {
                            module.$element.css('min-height', (prevMenu.innerHeight() / module.fontSize) + 'em');
                        } else {
                            module.$element.css('min-height', (0 / module.fontSize) + 'em');
                        }
                        module.$element.toggleClass('is-nested', module.depth>0);
                        $('html').toggleClass('is-nested-nav', module.depth > 0);
                    });
                });
            }
        },

        showCurrentPageNode: function(module) {
            var rootNavList = module.$element.find('.MainNav-list').eq(0),
                currentPageItem = $('.MainNav-item[data-page-id=' + module.settings.currentPage + ']'),
                parentItems = currentPageItem.parents('.MainNav-list');

            currentPageItem.addClass('is-active');
            module.depth = parentItems.length - 1;

            if (module.depth > 0) {
                parentItems.addClass('is-active');
                module.$element.css('min-height', (currentPageItem.closest('.MainNav-list').innerHeight() / module.fontSize) + 'em');
            } else {
                module.depth = 0;
                module.$element.css('min-height', (module.$element.find('.MainNav-list').eq(0) / module.fontSize) + 'em');
            }
            module.$element.toggleClass('is-nested', module.depth > 0);
            $('html').toggleClass('is-nested-nav', module.depth > 0);
        }
    });

})(jQuery);
;
(function () {
    "use strict";

    var moduleObj = moduler('modal', {
        defaults: {
            event: 'click', // the event to open the modal on
            contentElement: null, // selector for the  Modal component
            replaceHtmlAfterClose: true, // if true will restore the html to the initial state
            activeCssClass: 'Modal--active',
            reposition: null,
            blockBody: false,
            blockClass: 'block',
            blockSelector: 'body'
        },

        init: function (module) {
            module.$contentElement = $(module.settings.contentElement);
            module.contentElementHtml = module.$contentElement.html();

            moduleObj.initEvents(module);
        },

        initEvents: function (module) {
            $(window).keyup(function (event) {
                var modalIsOpen = module.$contentElement.is('.' + module.settings.activeCssClass);

                // user press the escape key
                if (modalIsOpen && event.keyCode == 27) {
                    moduleObj.closeDialog(module);
                }
            });

            // opening the modal
            module.$element.on(module.settings.event, function (e) {
                e.preventDefault();

                // move modal element to body to avoid z-index issues.
                if (!module.$contentElement.parent().is('body')) {
                    module.$contentElement.detach().appendTo('body');
                }

                module.$contentElement.addClass(module.settings.activeCssClass);

                moduleObj.listen.setPosition(module);
                module.$contentElement.add(module.$element).trigger('modal-open');

                if ($(module.settings.blockBody)) {
                    $(module.settings.blockSelector).addClass(module.settings.blockClass);
                    var scrollTop = $(window).scrollTop();

                    //ios hack as safari doesnt adhere to body > overflow:hidden, https://bugs.webkit.org/show_bug.cgi?id=153852
                    //so we bind the scroll function to a new event and set the current possition
                    //courtesy of bobo!
                    $(document).bind('scroll', function () { window.scrollTo(0, scrollTop); }); 
                }

            });

            // closing window by clicking close link
            module.$contentElement.on('click', '.js-close', function (e) {
                e.preventDefault();

                moduleObj.closeDialog(module);
            });

            if (module.settings.reposition) {
                module.$contentElement.on(module.settings.reposition, function (e) {
                    moduleObj.listen.setPosition(module);
                });
            }

            // closing window by clicking overlay
            module.$contentElement.on('click', function (e) {
                if (!$(e.target).is('.Modal'))
                    return;

                e.preventDefault();
                moduleObj.closeDialog(module);
            });
        },

        listen: {
            setPosition: function (module) {
                var scrollTop = $(window).scrollTop();
                var dialog = module.$contentElement.find('.Modal-dialog');

                dialog.css('top', scrollTop + "px");
            }
        },

        closeDialog: function (module) {
            module.$contentElement.removeClass(module.settings.activeCssClass);

            if (module.settings.replaceHtmlAfterClose) {
                module.$contentElement.html(module.contentElementHtml);

                if ('validator' in $ && 'unobtrusive' in $.validator) {
                    $.validator.unobtrusive.parse(module.settings.contentElement);
                }
            }
            if ($(module.settings.blockBody)) {
                $(module.settings.blockSelector).removeClass(module.settings.blockClass);
                $(document).unbind('scroll');
            }
            module.$contentElement.add(module.$element).trigger('modal-close');
        }
    });

})();;
(function () {
    "use strict";

    var moduleObj = moduler('poster', {
        defaults: {
            url: null,
            event: 'click',
            data: undefined,
            once: false,
            contentElement: null,
            responseModule: null,
            httpMethod: 'POST',
            loadingCssClass: 'is-loading'
        },
        
        init: function (module) {
            var bindMethod = module.settings.once ? "one" : "on";
            module.$element[bindMethod](module.settings.event, module, moduleObj.listen.fireRequest);
        },

        listen: {
            fireRequest: mo.event(function (module, e) {
                var settings = module.settings,
                    $contentElement = settings.contentElement !== null ? $(settings.contentElement) : module.$element;

                e.preventDefault();
                module.$element.addClass(settings.loadingCssClass);

                // send request
                $.ajax({
                    type: settings.httpMethod,
                    url: settings.url,
                    data: settings.data ? settings.data : {}
                }).always(function () {
                    module.$element.removeClass(settings.loadingCssClass);
                }).done(function (response, status, xhr) {
                    if (settings.responseModule) {
                        mo.utils.removeModuleFromElement($contentElement, settings.responseModule);
                        mo.utils.addModuleToElement($contentElement, settings.responseModule, { response: response });
                    } else if ($contentElement.length && xhr.getResponseHeader('content-type').indexOf('text/html') !== -1) {
                        $contentElement.html(response);
                    }

                    module.$element.trigger('poster-done', { response: response });
                }).error(function () {
                    module.$element.trigger('poster-error');
                });
    
            })
        },

        destroy: function (module) {
            module.$element.off(module.settings.event, moduleObj.listen.fireRequest);
        }
    });
    
})();;


(function () {
    "use strict";
    var moduleObj = moduler('recaptcha', {
        defaults: {
            id: new Date().getMilliseconds(),
            apiScriptUrl: 'https://www.google.com/recaptcha/api.js?render=explicit&onload=',
            publicKey: null,
            theme: null,
            focusOnField: false,
            captcha: null
        },

        init: function (module) {
            var functionName = 'recaptcha' + module.settings.id.replace(/-/g, ""),
		        apiUrl = module.settings.apiScriptUrl + functionName;
            //the API script url should have the 'onload=grecaptchaLoad' query so that the window.grecaptchaLoad gets called
            $.getScript(apiUrl, function () {
                moduleObj.scriptsLoaded(module, functionName);
            });

            window[functionName] = function () {

                $('[data-module="recaptcha"]').each(function () {
                    var el = this,
                        $el = $(this);
                    if ($el.prop('grecaptcha.done')) {
                        return;
                    }
                    $el.prop('grecaptcha.done', 'true');
                    grecaptcha.render(el.id, {
                        sitekey: module.settings.publicKey,
                        theme: module.settings.theme,
                        callback: function () {
                            //grecaptcha.reset();
                            //grecaptcha.reset(module.settings.captcha);
                            //module.settings.focusOnField ? grecaptcha.reset(module.settings.captcha) : $.noop;
                        }
                    });
                });

            };


        },
        scriptsLoaded: function (module) {
            //if (!window['grecaptcha']) {
            //    setTimeout(function () { moduleObj.scriptsLoaded(module); }, 1000);
            //    return;
            //}

            //$('[data-module="recaptcha"]').each(function () {
            //    var el = this,
            //        $el = $(this);
            //    if ($el.prop('grecaptcha.done')) {
            //        return;
            //    }
            //    $el.prop('grecaptcha.done', 'true');
            //    grecaptcha.render(el.id, {
            //        sitekey: module.settings.publicKey,
            //        theme: module.settings.theme,
            //        callback: function () {
            //            //grecaptcha.reset();
            //            grecaptcha.reset(module.settings.captcha);
            //            //module.settings.focusOnField ? grecaptcha.reset(module.settings.captcha) : $.noop;
            //        }
            //    });
            //});


        }
    });

})();
;
(function () {
    "use strict";

    var moduleObj = moduler('refresh', {
        defaults: {
            event: 'click',
        	loadingCssClass: 'is-loading',
        	refreshSegments: [],
        	tag: 'A',
        	attribute: 'href',
        	cache: true,
        	useHistoryApi: false,
            cookieKey: null
        },
        
        init: function (module) {
        	var settings = module.settings,
				eventElement = module.$element.prop('tagName') == settings.tag.toUpperCase() ? module.$element : module.$element.find(settings.tag);
        	
            eventElement.on(module.settings.event, function (e) {
                e.preventDefault();

                var url = $(this).attr(settings.attribute);

                $(this).addClass(settings.loadingCssClass);

                $.ajax({
                	url: url,
                	data: { partialPage: true },
                    cache: module.settings.cache
                }).always(function () {
                	$(this).removeClass(settings.loadingCssClass);
                }).done(function (response, status, xhr) {
                	var $response = $($.parseHTML(response));
                	
                	if (module.settings.useHistoryApi && 'history' in window && 'replaceState' in history) {
                	    history.replaceState(null, document.title, url);
                	}

                	if (module.settings.cookieKey && 'cookie' in $) {
                	    $.cookie(module.settings.cookieKey, decodeURIComponent(url), { path: '/' });
                	}

                	for (var i in module.settings.refreshSegments) {
                		var segment = module.settings.refreshSegments[i];
                		var $segment = $response.find(segment).add($response.filter(segment));

                		if ($segment.length) {
						    $(segment).html($segment.html());
						}
                    }
                }).error(function (response) {
                	
                });
            });
        }
    });
    
})();;
(function () {
    "use strict";

    var moduleObj = moduler('registration', {
        defaults: {},
        
        init: function (module) {

        	$('.js-registration-form', module.element).submit(function (e) {
        		if ($(this).valid()) {

        			if ($('.js-registration').data('registration-step') == 1) {
        				$('.js-registration').data('registration-step', 2);
        				$('.js-registrationStep2').removeClass('u-isHidden');
        				$('.js-registrationStep1').slideUp();
        				$('.js-registration-legend').html("Step 2 of 2");

        				$("html, body").animate({ scrollTop: 0 }, "slow");
        				e.preventDefault();
        			}
        		} else {
        			e.preventDefault();
        		}
        	});

        }
        
    });
    
})();;
(function () {
    "use strict";

    var moduleObj = moduler('save-school', {
        defaults: {
            isSavedSchool: null,
            schoolPageId: null, // if needed
            saveUrl: null,
            removeUrl: null,
            saveText: '',
            removeText: ''
        },

        init: function (module) {
            module.poster = mo.utils.getSettings(module.element, 'poster');
            moduleObj.updatePosterUrl(module);

            if (module.settings.schoolPageId != null) {
                module.poster.data = {
                    schoolPageId: module.settings.schoolPageId
                };
            }

            module.$element.on('poster-done', function (e, data) {
                if (data.response.success) {
                    module.settings.isSavedSchool = !module.settings.isSavedSchool;

                    moduleObj.updatePosterUrl(module);
                    moduleObj.updateButtonText(module);

                    module.$element.trigger('save-school-done', { response: data.response });
                } else {
                    module.$element.trigger('save-school-error', { response: data.response });
                }
            });
        },

        updateButtonText: function (module) {
            if (module.settings.isSavedSchool) {
                module.$element.html(module.settings.removeText);
            } else {
                module.$element.html(module.settings.saveText);
            }
        },

        updatePosterUrl: function (module) {
            if (module.settings.isSavedSchool) {
                module.poster.url = module.settings.removeUrl;
            } else {
                module.poster.url = module.settings.saveUrl;
            }
        }

    });

})();;
(function () {
    "use strict";

    var moduleObj = moduler('saved-schools-count', {
        defaults: {
            contentElement: null,
            badgeElement: '.js-schoolsBadge',
            headerElement: '.js-savedSchoolsLink',
            cssClass: 'u-sliderActive'
        },

        init: function (module) {
            module.$contentElement = $(module.settings.contentElement);
            module.$badgeElements = $(module.settings.badgeElement);
            module.$headerElement = $(module.settings.headerElement);

            module.$element.on('save-school-done', function (e, data) {
                var response = data.response;
                module.$badgeElements.text(response.count);
                if (response.count > 0) {
                    module.$headerElement.removeClass('u-isHidden');
                    module.$contentElement.addClass(module.settings.cssClass);
                } else {
                    module.$headerElement.addClass('u-isHidden')
                    module.$contentElement.removeClass(module.settings.cssClass);
                }
            });
        }
    });

})();;
(function () {
    "use strict";
    
    // This module is bound to the country dropdown

    var moduleObj = moduler('state-filter', {
        defaults: {
            data: [],
            stateSelector: '#SearchFields_State'
        },
        
        init: function (module) {
            module.$state = $(module.settings.stateSelector);
            module.$state.prop('default-values', module.$state.html());

            moduleObj.updateValues(module);

            if (module.$state.val()) {
                module.$state.prop('disabled', false);
            }

            module.$element.on('change', function (e) {
                moduleObj.updateValues(module);
                module.$state.val('');
            });
        },

        updateValues: function(module) {
            var currentValue = module.$element.find('option:checked').val();

            if (!currentValue) {
                return;
            }

            for (var i = 0; i < module.settings.data.length; i++) {
                var countryInfo = module.settings.data[i];

                if (countryInfo.country == currentValue) {
                    module.$state.html(module.$state.prop('default-values'));

                    module.$state.find('option').each(function (u, el) {
                        if (!el.value) {
                            return;
                        }

                        if (!moduleObj.hasState(countryInfo.states, el.value)) {
                            $(el).remove();
                        }
                    });

                    module.$state.removeAttr('disabled');
                    return;
                }
            }

            module.$state.attr('disabled', 'disabled').val('');
        },

        hasState: function(states, state) {
            for (var i in states) {
                if (states[i] == state) {
                    return true;
                }
            }
            return false;
        }
    });
    
})();;
(function () {
    "use strict";

    var moduleObj = moduler('state-toggler', {
        defaults: {
            stateSelector: null
        },
        
        init: function (module) {
            module.$element.on('change', function (e) { 
                if ($(this).val() == 'US') { 
                    $(module.settings.stateSelector).find("select").prop('disabled', false);
                } else {
                    $(module.settings.stateSelector).find("select").prop('disabled', true);
                }

               

            })

         
        }

        
        
          
    });
    
})
();
;
(function () {
    "use strict";

    var moduleObj = moduler('sticky', {
        defaults: {
            stickyHeightLimit: 0, // when to add the class
            cssClass: 'is-shrunk',
            heightElement: ''
        },

        init: function (module) {
            var scrollFired = false;
            var prevScroll = getScrollY();

            $(window).scroll(function(event) {
                if (!scrollFired) {
                    scrollFired = true;
                    setTimeout(scrollPage, 50);
                }
            });

            function scrollPage() {
                var sy = getScrollY();
                // if 'heightElement' is set up it will overwrite 'stickyHeightLimit'
                var stickyHeight = $("." + module.settings.heightElement);

                if (stickyHeight.length > 0) {
                    module.settings.stickyHeightLimit = stickyHeight.height()
                }

                if(sy >= prevScroll && sy >= module.settings.stickyHeightLimit) {
                    module.$element.addClass(module.settings.cssClass);
                } else if(sy < module.$element.height()) {
                    module.$element.removeClass(module.settings.cssClass);
                }

                prevScroll = sy;
                scrollFired = false;
            }

            scrollPage();

            function getScrollY() {
                return window.pageYOffset || document.documentElement.scrollTop;
            }
        }
    });

})();
;
(function() {
    "use strict";

    /*
        Example HTML:

        <div class="tabs" data-module="tabs">
            <ul class="tabs-handles">
                <li class="tabs-handle" data-name="tab-one">Tab One</li>
                <li class="tabs-handle" data-name="tab-two">Tab Two</li>
            </ul>

            <div class="tabs-panels">
                <div class="tabs-panel">
                    <!-- panel 1 -->
                </div>

                <div class="tabs-panel">
                    <!-- panel 2 -->
                </div>
            </div>
        </div>
    */

    var moduleObj = moduler('tabs', {
        defaults: {
            event: 'click',
            handles: '.Tabs-list > .Tabs-item',
            panels: '.Tabs-panel',
            activeClass: 'is-active',
            togglable: false, // whether to allow a panel to close if you click on its handle again.
            showFirst: true, // whether to show first panel on load.
            selectedPanel: null // the panel to show at page load (optional).
        },

        init: function(module) {
            module.$handles = module.$element.find(module.settings.handles);
            module.$panels = module.$element.find(module.settings.panels);

            module.$handles.on(module.settings.event, module, moduleObj.listen.showPanel);

            if (module.settings.selectedPanel) {
                module.$handles.filter('[data-name=' + module.settings.selectedPanel + ']').trigger(module.settings.event);
            } else if (module.settings.showFirst) {
                module.$handles.first().trigger(module.settings.event);
            }
        },

        listen: {
            showPanel: mo.event(function(module, e) {
                e.preventDefault();

                var $handle = $(e.target),
                    settings = module.settings,
                    handleIndex = module.$handles.index($handle),
                    $panel = module.$panels.eq(handleIndex);

                // if event was triggered on a link inside "handles" then we try to find the handle element now
                if (!$handle.is(module.settings.handles)) {
                    $handle = $handle.closest(module.settings.handles);
                    handleIndex = module.$handles.index($handle);
                    $panel = module.$panels.eq(handleIndex);
                }

                module.$handles.not($handle).removeClass(settings.activeClass).trigger('tab-hide');
                module.$panels.not($panel).removeClass(settings.activeClass).trigger('tab-hide');

                var classFuncName = settings.togglable ? 'toggleClass' : 'addClass';
                $handle[classFuncName](settings.activeClass).trigger('tab-show');
                $panel[classFuncName](settings.activeClass).trigger('tab-show');
            })
        },

        destroy: function(module) {
            module.$handles.off(module.settings.event, moduleObj.listen.showPanel);
        }
    });

})();;
(function ($) {
    "use strict";

    var moduleObj = moduler('toggler', {
        defaults: {
            event: 'click',
            contentElement: null,
            cssClass: 'u-isHidden',
            preventDefault: true,
            once: false,
            toggleOnBlur: false
        },

        init: function (module) {
            module.$contentElement = $(module.settings.contentElement ? module.settings.contentElement : module.element);
            module.$element.on(module.settings.event, module, moduleObj.listen.toggleVisibility);

            if (module.settings.toggleOnBlur) {
                document.addEventListener('click', function (e) {
                    if (e && e.target && !e.target.closest(module.settings.contentElement)) {
                        moduleObj.listen.show(module);
                    }
                })
            }
        },

        listen: {
            show: mo.event(function (module) {
                module.$contentElement.removeClass(module.settings.cssClass);
                module.$contentElement.trigger('toggler-shown');
            }),

            hide: mo.event(function (module) {
                module.$contentElement.addClass(module.settings.cssClass);
                module.$contentElement.trigger('toggler-hidden');
            }),

            toggleVisibility: mo.event(function (module, e) {
                if (module.settings.preventDefault) {
                    e.preventDefault();
                }

                if (module.settings.once && module.toggled) {
                    return;
                } else if (module.settings.once) {
                    module.toggled = true;
                }

                module.$contentElement.toggleClass(module.settings.cssClass);
                module.$contentElement.trigger('toggler-toggled');
            })
        }
    });

})(jQuery);
;

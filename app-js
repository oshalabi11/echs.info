this,"clusteringend",this);for(o=0;o<this.clusters_.length;o++)this.clusters_[o].updateIcon()}}},e.CALCULATOR=function(t,e){for(var r=0,n=t.length,i=n;0!==i;)i=Math.floor(i/10),r++;return r=Math.min(r,e),{text:n.toString(),index:r,title:""}},e.withDefaultStyle=function(t){return rr({textColor:"black",textSize:11,textDecoration:"none",textLineHeight:t.height,fontWeight:"bold",fontStyle:"normal",fontFamily:"Arial,sans-serif",backgroundPosition:"0 0"},t)},e.BATCH_SIZE=2e3,e.BATCH_SIZE_IE=500,e.IMAGE_PATH="../images/m",e.IMAGE_EXTENSION="png",e.IMAGE_SIZES=[53,56,66,78,90],e}(qr)}();
;
/*!
 * Layout Engine v0.8.3
 *
 * Adds the rendering engine and browser names as a class on the html tag and returns a JavaScript object containing the vendor, version and browser name (where appropriate)
 *
 * Possible vendors: '.vendor-' + 'ie', 'khtml', 'mozilla', 'opera', 'webkit'
 * '.vendor-ie' also adds the version: 'vendor-' + 'ie-11', 'ie-10', 'ie-9', 'ie-8', 'ie-7'
 * '.vendor-opera-mini' is also detected
 *
 * Possible browsers: '.browser-' + 'android', 'chrome', 'wiiu'
 *
 * Copyright (c) 2014 Matt Stow
 *
 * http://mattstow.com
 *
 * Licensed under the MIT license
 */;
var layoutEngine = (function () {
    var html = document.documentElement,
        style = html.style,
        vendor = ' vendor-',
        ie = 'ie',
        khtml = 'khtml',
        mozilla = 'mozilla',
        opera = 'opera',
        webkit = 'webkit',
        browser = ' browser-',
        android = 'android',
        chrome = 'chrome',
        wiiu = 'wiiu',
        cssClass = vendor,
        jsObject;

    // IE
    if ('-ms-scroll-limit' in style || 'behavior' in style) {
        cssClass += ie + vendor + ie;
        if ('-ms-ime-align' in style) {
            cssClass += '-11'
            jsObject = {
                vendor: ie,
                version: 11
            }
        } else if ('-ms-user-select' in style) {
            cssClass += '-10'
            jsObject = {
                vendor: ie,
                version: 10
            }
        } else if ('fill' in style) {
            cssClass += '-9';
            jsObject = {
                vendor: ie,
                version: 9
            }
        } else if ('widows' in style) {
            cssClass += '-8';
            jsObject = {
                vendor: ie,
                version: 8
            }
        } else {
            cssClass += '-7';
            jsObject = {
                vendor: ie,
                version: 7
            }
        }
    }
        // WebKit
    else if ('WebkitAppearance' in style) {
        cssClass += webkit;
        var ua = navigator.userAgent;

        if (ua.indexOf('Android') >= 0 && ua.indexOf('Chrome') === -1) {
            cssClass += browser + android;
            jsObject = {
                vendor: webkit,
                browser: android
            }
        } else if (!!window.chrome || ua.indexOf('OPR') >= 0) {
            cssClass += browser + chrome;
            jsObject = {
                vendor: webkit,
                browser: chrome
            }
        } else if (!!window.wiiu) {
            cssClass += browser + wiiu;
            jsObject = {
                vendor: webkit,
                browser: wiiu
            }
        } else {
            jsObject = {
                vendor: webkit
            }
        }
    }
        // Mozilla
    else if ('MozAppearance' in style) {
        cssClass += mozilla;
        jsObject = {
            vendor: mozilla
        }
    }
        // Opera
    else if ('OLink' in style || !!window.opera) {
        cssClass += opera;
        if ('OMiniFold' in style) {
            cssClass += '-mini';
            jsObject = {
                vendor: opera,
                version: 'mini'
            }
        } else {
            jsObject = {
                vendor: opera
            }
        }
    }
        // KHTML
    else if ('KhtmlUserInput' in style) {
        cssClass += khtml;
        jsObject = {
            vendor: khtml
        }
    } else {
        return false;
    }

    html.className += cssClass;

    return jsObject;
})();;
(function () {
    "use strict";

    /*
        Example HTML:

        <div class="accordion" data-module="accordion">
            <h3 class="accordion-header">Panel 1</h3>
            <div class="accordion-panel">
                <!-- content 1 -->
            </div>

            <h3 class="accordion-header">Panel 2</h3>
            <div class="accordion-panel">
                <!-- content 2 -->
            </div>
        </div>
    */

    var moduleObj = moduler('accordion', {
        defaults: {
            event: 'click',
            headers: '.Accordion-header',
            panels: '.Accordion-panel',
            activeClass: 'is-active',
            mode: 'accordion', // accordion|toggle
            slide: true,
            slideDelay: 400,
            expandFirst: false,
            selectedPanel: null // the panel to show at page load (optional).
        },
        
        init: function (module) {
            module.$headers = module.$element.find(module.settings.headers);
            module.$panels = module.$element.find(module.settings.panels);

            if (module.settings.slide) {
                module.$panels.hide();
            }

            module.$headers.on(module.settings.event, module, moduleObj.listen.expandPanel);

            module.$element.addClass('js-ready');

            if (module.settings.selectedPanel) {
                module.$headers.filter('[data-name=' + module.settings.selectedPanel + ']').trigger(module.settings.event);
            } else if (module.settings.expandFirst) {
                module.$headers.first().trigger(module.settings.event);
            }
        },
        
        listen: {
            expandAll: mo.event(function (module) {
                module.$headers.addClass(module.settings.activeClass).trigger('accordion-expand');
                module.$panels.addClass(module.settings.activeClass).trigger('accordion-expand');
                
                if (module.settings.slide) {
                    module.$panels.slideDown(module.settings.slideDelay);
                }
            }),

            collapseAll: mo.event(function (module, e) {
                module.$headers.removeClass(module.settings.activeClass).trigger('accordion-collapse');
                module.$panels.removeClass(module.settings.activeClass).trigger('accordion-collapse');
            
                if (module.settings.slide) {
                    module.$panels.slideUp(module.settings.slideDelay);
                }
            }),

            expandPanel: mo.event(function (module, e) {
                e.preventDefault();
                
                var $header = $(e.target),
                    settings = module.settings,
                    headerIndex = module.$headers.index($header),
                    $panel = module.$panels.eq(headerIndex);

                // if event was triggered on a link inside "headers" then we try to find the handle element now
                if (!$header.is(module.settings.handles)) {
                    $header = $header.closest(module.settings.headers);
                    headerIndex = module.$headers.index($header);
                    $panel = module.$panels.eq(headerIndex);
                }

                if (settings.mode == 'accordion') {
                    module.$headers.not($header).removeClass(settings.activeClass).trigger('accordion-collapse');
                    module.$panels.not($panel).removeClass(settings.activeClass).trigger('accordion-collapse');
                    
                    if (settings.slide) {
                        module.$panels.not($panel).slideUp(settings.slideDelay);
                    }
                }

                $header.toggleClass(settings.activeClass).trigger('accordion-expand');
                $panel.toggleClass(settings.activeClass).trigger('accordion-expand');
                
                if (settings.slide) {
                    $panel.stop(true).slideToggle(settings.slideDelay);
                }
            })
        },
        
        destroy: function (module) {
            module.$headers.off(module.settings.event, moduleObj.listen.expandPanel);
        }
    });
    
})();;
(function () {
    "use strict";

    var moduleObj = moduler('anniversary-main-video-resize', {
        defaults: {
            startPageSelector: '.Startpage'
        },

        init: function (module) {
            function videoResize() {
                var startPage = $(module.settings.startPageSelector);
                var video = module.$element;

                var videoWidth = video[0].videoWidth;
                var videoHeight = video[0].videoHeight;
                var videoRatio = videoWidth / videoHeight;

                var windowWidth = $(window).width();
                var windowHeight = $(window).height();
                var windowRatio = windowWidth / windowHeight;

                if (videoRatio > windowRatio) {
                    video.width('auto');
                    video.height(windowHeight);
                    startPage.height(windowHeight);
                } else {
                    video.width(windowWidth);
                    video.height(video.width() / videoRatio);
                    startPage.height(windowHeight);
                }
            }

            module.$element.on('loadedmetadata', function () {
                videoResize();
                $(window).scroll();
            });
            module.$element.one('playing', function () {
                $('.Startpage-loader').fadeOut(400);
            });


            $(window).on('resize', function () {
                var resizeTimer;
                clearTimeout(resizeTimer);
                resizeTimer = setTimeout(function () {
                    videoResize();
                }, 100);
            });
        }
    });
})();;
(function () {
    "use strict";

    var moduleObj = moduler('anniversary-timeline-counter', {
        defaults: {
            eventBlocksSelector: null, // selector for timeline events blocks
            yearCounterSelector: null, // selector for year counter
            schoolsCounterSelector: null, // selector for schools counter
            programsCounterSelector: null, // selector for programs counter
            yearDataAttribute: 'year', // data attribute for year count
            schoolsDataAttribute: 'schools', // data attribute for schools count
            programsDataAttribute: 'programs' // data attribute for programs count
        },

        init: function (module) {
            $(window).on('scroll', module, moduleObj.listen.countAttributes);

            var countersFromHash = moduleObj.getCountersFromHash(module);

            if (countersFromHash != null) {
                moduleObj.setCounters(module, countersFromHash);
            } else {
                moduleObj.listen.countAttributes(module);
            }
        },

        getCountersFromHash: function (module) {
            var hash = window.location.hash.substring(1);

            var year = parseInt(hash);
            if (!year || year < 1925 || year > 2100) {
                return null;
            }

            var schools = $('[data-' + module.settings.yearDataAttribute + '=' + year + ']').first()
                .data(module.settings.schoolsDataAttribute);
            var programs = $('[data-' + module.settings.yearDataAttribute + '=' + year + ']').first()
                .data(module.settings.programsDataAttribute);

            return {
                year: year,
                schools: schools,
                programs: programs
            };

        },

        setCounters: function (module, counters) {
            $(module.settings.yearCounterSelector).text(counters.year);
            $(module.settings.schoolsCounterSelector).text(counters.schools);
            $(module.settings.programsCounterSelector).text(counters.programs);
            if (counters.hash) {
                window.location.hash = counters.hash;
            }
        },

        listen: {
            countAttributes: mo.event(function (module) {
                //var currentViewTop = $(window).scrollTop();
                var currentViewMiddleScreen = $(window).scrollTop() + $(window).height()/2;
                var eventBlocksTop = $(module.settings.eventBlocksSelector).map(function () {
                    return Math.floor($(this).offset().top);
                }).get();


                for (var i = 0; i < eventBlocksTop.length; i++) {
                    if (eventBlocksTop[i] < currentViewMiddleScreen) {
                        var year = $($(module.settings.eventBlocksSelector)[i]).data(module.settings.yearDataAttribute);
                        var schools = $($(module.settings.eventBlocksSelector)[i]).data(module.settings.schoolsDataAttribute);
                        var programs = $($(module.settings.eventBlocksSelector)[i]).data(module.settings.programsDataAttribute);
                    }
                }

                moduleObj.setCounters(
                    module,
                    {
                        year: year,
                        schools: schools,
                        programs: programs,
                        hash: year
                    }
                );
            })
        }
    });
})();;
(function () {
  "use strict";

  var moduleObj = moduler('anniversary-timeline-progress-bar', {
    defaults: {},

    init: function (module) {
      $(window).on('scroll', module, moduleObj.listen.updateTimeline);

      moduleObj.listen.updateTimeline(module);
    },

    listen: {
      updateTimeline: mo.event(function (module) {
        var pageHeight = $(document).height() - $(window).height();
        var currentViewTop = $(window).scrollTop();
        var progress = Math.round(currentViewTop / pageHeight * 100);
        module.$element.width(progress + '%');
      })
    }
  });
})();;
(function () {
    "use strict";

    var moduleObj = moduler('anniversary-video-controls', {
        defaults: {
            videoSelector: 'video',
            videoControlsSelector: '.Video-control',
            videoControlButtonsSelector: '.Video-control-buttons',
            videoPlayButtonSelector: '.Video-play',
            playButtonSelector: '.btnPlay',
            subtitlesButtonSelector: '.btnSubtitles',
            volumeButtonSelector: '.btnVolume',
            enlargeButtonSelector: '.btnEnlarge',
            playClass: 'play',
            pauseClass: 'pause',
            muteClass: 'mute'
        },

        init: function (module) {
            var userOverride = false;
            var videoControls = module.$element.find(module.settings.videoControlsSelector)[0];
            var videoPlayerElement = module.$element.find(module.settings.videoSelector)[0];
            var playControlButton = module.$element.find(module.settings.playButtonSelector)[0];
            var volumeControlButton = module.$element.find(module.settings.volumeButtonSelector)[0];

            // turn off all subtitles
            for (var i = 0; i < videoPlayerElement.textTracks.length; i++) {
                videoPlayerElement.textTracks[i].mode = 'hidden';
            }
            //create timeout event to hide controls after mouse has been still
            var timer = setTimeout(function () {
                console.log('timeout');
                hidePlayerControls();
            }, 4000);

            $(module.settings.videoSelector).on('mousemove', function () {
                console.log('mousemove');
                clearTimeout(timer);
                showPlayerControls();
                timer = setTimeout(function () {
                    console.log('timeout');
                    hidePlayerControls();
                }, 4000);
            });

            function isVideoPlaying() {
                return videoPlayerElement.currentTime > 0 &&
                    !videoPlayerElement.paused &&
                    !videoPlayerElement.ended;
            }

            function play(userClick) {
                if (userClick || !userOverride) {
                    if (!isVideoPlaying()) {
                        videoPlayerElement.play();
                    }
                }
            }

            function pause(userClick) {
                if (userClick || !userOverride) {
                    if (isVideoPlaying()) {
                        videoPlayerElement.pause();
                    }
                }
            }

            function togglePlay() {
                userOverride = true;
                if (videoPlayerElement.paused) {
                    play(true);
                } else {
                    pause(true);
                }
                return false;
            }

            function controlVolume() {
                videoPlayerElement.muted = !videoPlayerElement.muted;
                if (videoPlayerElement.muted) {
                    $(volumeControlButton).addClass(module.settings.muteClass);
                } else {
                    $(volumeControlButton).removeClass(module.settings.muteClass);
                }
                return false;
            }


            //subtitles stuff
            //show hide dialog with subs...
            $(module.settings.subtitlesButtonSelector).on('click', function (e) {
                e.preventDefault();
                e.stopPropagation();
                $('.SubtitlesList').show();

            });

            $(window).on('click', function () {
                $('.SubtitlesList').hide();
            });

            //enable subs
            $('.SubtitlesList li.enable').on('click', function (e) {
                $(this).addClass('active');
                $('.SubtitlesList li.disable').removeClass('active');
                //$(videoPlayerElement).find('track').prop('mode', 'showing');
                videoPlayerElement.textTracks[0].mode = 'showing';
                $('.Startpage-discover').addClass('hidden');
                $('.SubtitlesList').hide();
            });
            //disable subs
            $('.SubtitlesList li.disable').on('click', function (e) {
                //$(videoPlayerElement).find('track').prop('mode', 'hidden');
                $('.SubtitlesList li.enable').removeClass('active');
                $(this).addClass('active');
                videoPlayerElement.textTracks[0].mode = 'hidden';
                $('.Startpage-discover').removeClass('hidden');
                $('.SubtitlesList').hide();
            });

            function controlSubtitles() {
                $('.SubtitlesList').fadeToggle();
                if (videoPlayerElement.textTracks[0].mode == 'hidden') {
                    videoPlayerElement.textTracks[0].mode = 'showing';
                    $('.Startpage-discover').addClass('hidden');
                } else {
                    videoPlayerElement.textTracks[0].mode = 'hidden';
                    $('.Startpage-discover').removeClass('hidden');
                }
                console.log(videoPlayerElement.textTracks.length);
            }

            function controlSize() {
                if (videoPlayerElement.requestFullscreen) {
                    console.log('fullscreen - requestFullscreen');
                    videoPlayerElement.requestFullscreen();
                } else if (videoPlayerElement.mozRequestFullScreen) {
                    console.log('fullscreen - mozRequestFullScreen');
                    videoPlayerElement.mozRequestFullScreen();
                } else if (videoPlayerElement.webkitRequestFullscreen) {
                    console.log('fullscreen - webkitRequestFullscreen');
                    videoPlayerElement.webkitRequestFullscreen();
                } else if (videoPlayerElement.webkitEnterFullscreen) {
                    console.log('fullscreen - webkitEnterFullscreen');
                    videoPlayerElement.webkitEnterFullscreen();
                }
                if (videoPlayerElement.muted) {
                    controlVolume();
                }
            }

            function hidePlayerControls() {
                $(module.$element).find(module.settings.videoControlButtonsSelector).fadeOut();
            }

            function showPlayerControls() {
                $(module.$element).find(module.settings.videoControlButtonsSelector).fadeIn();
            }

            function isVideoInView() {
                var videoTop = $(module.$element).offset().top;
                var videoBottom = $(module.$element).offset().top + $(module.$element).height();
                var currentViewTop = $(window).scrollTop();
                var currentViewBottom = $(window).scrollTop() + $(window).height();

                return videoTop > currentViewTop &&
                    videoTop < currentViewBottom &&
                    videoBottom > currentViewTop &&
                    videoBottom < currentViewBottom;
            }

            function isMobile() {
                return window.matchMedia("only screen and (max-width: 560px)").matches;
            }

            function controlVideoAutoplay() {
                if (!isMobile() && isVideoInView()) {
                    play();
                } else {
                    pause();
                }
            }

            if (isMobile()) {
                showPlayerControls();
                $(videoPlayerElement).removeAttr("autoplay");
                $(videoPlayerElement).attr("preload", "none");
                $(videoControls).removeClass(module.settings.playClass).addClass(module.settings.pauseClass);
            }

            $(module.$element).find(module.settings.videoControlsSelector).on('click', "".concat(module.settings.playButtonSelector, ", ", module.settings.videoPlayButtonSelector), togglePlay);
            $(module.$element).find(module.settings.videoControlsSelector).on('click', module.settings.volumeButtonSelector, controlVolume);
            $(module.$element).find(module.settings.videoControlsSelector).on('click', module.settings.enlargeButtonSelector, controlSize);
            //$(module.$element).find(module.settings.videoControlsSelector).on('click', module.settings.subtitlesButtonSelector, controlSubtitles);

            $(module.$element).on('mouseenter', showPlayerControls);
            $(module.$element).on('mouseleave', hidePlayerControls);

            $(videoPlayerElement).on('playing', function () {
                $(videoControls).removeClass(module.settings.pauseClass).addClass(module.settings.playClass);
            });
            $(videoPlayerElement).on('pause', function () {
                $(videoControls).removeClass(module.settings.playClass).addClass(module.settings.pauseClass);
            });

            $(window).on('scroll', controlVideoAutoplay);
        }
    });
})();;

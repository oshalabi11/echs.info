  .Heading--page,
  .Heading--pageWithBorder {
    font-size: 3.125em;
  }
}
.Heading--pageWithBorder {
  border-bottom: 1px solid #ccc;
  padding-bottom: 0.5em;
  margin-bottom: 0.8em;
}
